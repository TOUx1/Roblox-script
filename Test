-- TOU HUB v34 ULTIMATE — FINAL DECORATED VERSION 2025
-- City Thailand 2 | UI ดุเดือดเต็มขั้น | ชื่อเรนโบว์ 2 แบบ | ปิดแล้วหยุดจริง 100%

local NothingLibrary = loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/3345-c-a-t-s-u-s/NOTHING/main/source.lua'))()
local Notify = NothingLibrary.Notification()

local NotifyEnabled = true
local AutoRejoinEnabled = true
local AutoResumeFarmEnabled = true

local FarmStates = { Sell = false, Box = false, Medicine = false }
_G.SellToggle = nil
_G.BoxToggle = nil
_G.MedToggle = nil
_G.FarmLoop = nil

local IsWaveRainbow = false
local IsMusicRainbow = false
local WaveLoop = nil
local MusicLoop = nil

local Remote = game:GetService("ReplicatedStorage"):FindFirstChild("NameTagRemoteEvent")

local function SafeNotify(data)
	if NotifyEnabled then Notify.new(data) end
end

SafeNotify({Title="TOU HUB v34 ULTIMATE", Description="โหลดสำเร็จ! UI ดุเดือดสุดในไทย!", Duration=12, Icon="rbxassetid://8997385628"})

local Windows = NothingLibrary.new({
	Title = "TOU HUB v34 ULTIMATE",
	Description = "City Thailand 2 — ฟาร์มรวย ชื่อวิ่งสวย ลำโพงกระหึ่ม!",
	Keybind = Enum.KeyCode.LeftControl,
	Logo = 'http://www.roblox.com/asset/?id=18898582662'
})

-- ====================== Character Safe ======================
local function GetHumanoid()
	local plr = game.Players.LocalPlayer
	local char = plr.Character or plr.CharacterAdded:Wait()
	return char:WaitForChild("Humanoid", 10)
end

local function GetRootPart()
	local plr = game.Players.LocalPlayer
	local char = plr.Character or plr.CharacterAdded:Wait()
	return char:WaitForChild("HumanoidRootPart", 10)
end

-- ====================== Auto Rejoin + Resume ======================
local TeleportService = game:GetService("TeleportService")
local PlaceId = game.PlaceId

local function RejoinAndResume()
	if not AutoRejoinEnabled then return end
	SafeNotify({Title="โดน KICK แล้ว!", Description="กำลัง Rejoin + ฟาร์มต่อใน 5 วิ... ไม่ยอมแพ้!", Duration=6})
	task.wait(5)
	TeleportService:Teleport(PlaceId, game.Players.LocalPlayer)
end

game:GetService("GuiService").ErrorMessageChanged:Connect(RejoinAndResume)
TeleportService.TeleportInitFailed:Connect(RejoinAndResume)
game.Players.LocalPlayer.OnTeleport:Connect(function(state)
	if state == Enum.TeleportState.Failed then RejoinAndResume() end
end)

game.Players.LocalPlayer.CharacterAdded:Connect(function()
	if AutoResumeFarmEnabled then
		task.wait(3)
		if FarmStates.Sell and _G.SellToggle then _G.SellToggle:Callback(true) end
		if FarmStates.Box and _G.BoxToggle then _G.BoxToggle:Callback(true) end
		if FarmStates.Medicine and _G.MedToggle then _G.MedToggle:Callback(true) end
		SafeNotify({Title="กลับมาแล้ววว!", Description="ฟาร์มรันต่อทันที!", Duration=5})
	end
end)

-- ====================== Farm Tab ======================
local FarmTab = Windows:NewTab({Title = "ฟาร์มเงิน", Description = "ฟาร์มดุ ขายบ้านรัว รวยล้นเซิร์ฟ!", Icon = "rbxassetid://7733960981"})

local FarmLeft = FarmTab:NewSection({Title = "ออโต้ฟาร์มสุดโหด", Position = "Left"})
local AutoHouseSection = FarmTab:NewSection({Title = "ขายบ้านรัว ๆ", Position = "Right"})
local FarmRight = FarmTab:NewSection({Title = "ตั้งค่าสปีด & Noclip", Position = "Right"})
local FarmInfo = FarmTab:NewSection({Title = "ข้อมูลฮับ", Position = "Right"})

local URL_FARM = "https://raw.githubusercontent.com/TOUx1/Roblox-script/refs/heads/main/Sell"
local URL_BOX  = "https://pastefy.app/VaHDLWjU/raw"
local URL_MED  = "https://pastefy.app/ji38FSCI/raw"

local function SilentLoad(url)
	task.spawn(function()
		pcall(function() loadstring(game:HttpGet(url))() end)
	end)
end

AutoHouseSection:NewButton({Title = "ขายบ้านรัว ๆ", Description = "กดทีเดียว = เงินเป็นล้าน!", Callback = function()
	SafeNotify({Title="ขายบ้านเริ่มแล้ว!", Description="เงินกำลังมาเป็นแสนล้าน!", Duration=6})
	pcall(function() loadstring(game:HttpGet("https://pastefy.app/8V7h6QG3/raw"))() end)
end})

_G.SellToggle = FarmLeft:NewToggle({Title = "Auto Farm (ซื้อบ้านก่อน)", Default = false, Callback = function(state)
	FarmStates.Sell = state
	if state then
		SafeNotify({Title="FARM เปิดแล้ว!", Description="ขายของทุก 0.05 วิ รัวสุด!", Duration=4})
		SilentLoad(URL_FARM)
		_G.FarmLoop = game:GetService("RunService").Heartbeat:Connect(function()
			if not FarmStates.Sell then return end
			SilentLoad(URL_FARM)
			task.wait(0.05)
		end)
	else
		if _G.FarmLoop then _G.FarmLoop:Disconnect() _G.FarmLoop = nil end
		SafeNotify({Title="FARM ปิดแล้ว", Duration=2})
	end
end})

_G.BoxToggle = FarmLeft:NewToggle({Title = "ออโต้ส่งกล่อง", Default = false, Callback = function(state)
	FarmStates.Box = state
	if state then
		SafeNotify({Title="กล่องเริ่มส่งแล้ว!", Description="ทุก 15 วิ ได้ของแน่นอน", Duration=4})
		SilentLoad(URL_BOX)
		task.spawn(function()
			while FarmStates.Box do
				task.wait(15)
				if not FarmStates.Box then break end
				SilentLoad(URL_BOX)
			end
		end)
	else
		SafeNotify({Title="กล่องหยุดแล้ว", Duration=2})
	end
end})

_G.MedToggle = FarmLeft:NewToggle({Title = "ออโต้จ่ายยา", Default = false, Callback = function(state)
	FarmStates.Medicine = state
	if state then
		SafeNotify({Title="จ่ายยาเปิดแล้ว!", Description="ทุก 1 วิ เงินเข้าไม่หยุด!", Duration=4})
		SilentLoad(URL_MED)
		task.spawn(function()
			while FarmStates.Medicine do
				task.wait(1)
				if not FarmStates.Medicine then break end
				SilentLoad(URL_MED)
			end
		end)
	else
		SafeNotify({Title="จ่ายยาหยุดแล้ว", Duration=2})
	end
end})

FarmRight:NewSlider({Title = "WalkSpeed", Min = 16, Max = 500, Default = 16, Callback = function(v) 
	local hum = GetHumanoid() if hum then hum.WalkSpeed = v end 
end})

FarmRight:NewSlider({Title = "JumpPower", Min = 50, Max = 800, Default = 50, Callback = function(v) 
	local hum = GetHumanoid() if hum then hum.JumpPower = v end 
end})

FarmRight:NewKeybind({Title = "Noclip (กดค้าง)", Default = Enum.KeyCode.N, Callback = function()
	local noclip = false
	local conn
	conn = game:GetService("RunService").Stepped:Connect(function()
		if not noclip then conn:Disconnect() return end
		local root = GetRootPart()
		if root and root.Parent then
			for _, part in pairs(root.Parent:GetDescendants()) do
				if part:IsA("BasePart") and part ~= root then
					part.CanCollide = false
				end
			end
		end
	end)
	noclip = not noclip
	if noclip then SafeNotify({Title="Noclip เปิดแล้ว!", Description="ลอดกำแพงได้ทุกที่!", Duration=3}) end
end})

FarmInfo:NewTitle("TOU HUB v34 ULTIMATE")
FarmInfo:NewButton({Title = "Discord (แท้ 100%)", Callback = function() 
	setclipboard("https://discord.gg/cj93WrHEaf") 
	SafeNotify({Title="คัดลอกลิงก์แล้ว!", Description="มาเจอกันในดิส!", Duration=4}) 
end})

-- ==================== ส่วนที่ 1/2 จบ ====================

พิมพ์ "ต่อ" หรือ "2" เพื่อรับส่วนที่ 2/2 (รวม Warp + FE + ชื่อเรนโบว์ + ตั้งค่า) ได้เลยครับพี่ชาย!
-- ==================== ส่วนที่ 2/2 เริ่ม ====================

-- ====================== Warp Tab ======================
local WarpTab = Windows:NewTab({Title = "วาปด่วน 11 จุด", Description = "ไปไหนก็ถึงในเสี้ยววินาที!", Icon = "rbxassetid://7743866277"})
local WarpSection = WarpTab:NewSection({Title = "จุดวาปสำคัญ", Position = "Left"})

local function TeleportTo(pos, name)
	local root = GetRootPart()
	if root then
		local old = {}
		for _, p in pairs(root.Parent:GetDescendants()) do
			if p:IsA("BasePart") and p ~= root then old[p] = p.CanCollide p.CanCollide = false end
		end
		task.wait(0.1)
		root.CFrame = CFrame.new(pos)
		task.wait(0.4)
		for p, c in pairs(old) do if p and p.Parent then p.CanCollide = c end end
		SafeNotify({Title="วาปสำเร็จ!", Description="ไป " .. name .. " แล้ว!", Duration=3})
	end
end

local warps = {
	{"สถานีตำรวจ", Vector3.new(173.66, 177.31, 248.56)},
	{"บ้านตัวเอง", Vector3.new(251.54, 167.46, 104.53)},
	{"ร้านปืน M4", Vector3.new(383.23, 169.29, 294.55)},
	{"วัด", Vector3.new(434.08, 173.84, -47.66)},
	{"ขายรถ", Vector3.new(369.82, 171.46, -202.83)},
	{"ร้านข้าว", Vector3.new(-30.64, 178.42, 223.76)},
	{"โรงเรียน", Vector3.new(-116.03, 165.30, 160.62)},
	{"โรงพยาบาล", Vector3.new(-164.13, 177.18, 216.73)},
	{"ยิงจรวด", Vector3.new(-326.59, 263.38, 230.80)},
	{"เซเว่น", Vector3.new(356.42, 168.22, 42.65)},
	{"ปืนใต้ดิน", Vector3.new(-75.99, 163.69, 220.78)}
}

for _, w in ipairs(warps) do
	WarpSection:NewButton({Title = w[1], Callback = function() TeleportTo(w[2], w[1]) end})
end

-- ====================== FE TAB — ชื่อเรนโบว์ + ลำโพง ======================
local FETab = Windows:NewTab({Title = "FE ดุเดือด", Description = "ลำโพงดิสโก้ + ชื่อวิ่ง 2 สไตล์ สะเทือนทั้งเซิร์ฟ!", Icon = "rbxassetid://7733993369"})

local SpeakerSection = FETab:NewSection({Title = "ลำโพงดิสโก้", Position = "Left"})
local NameSection = FETab:NewSection({Title = "ชื่อเรนโบว์ทั้งหมด", Position = "Right"})

-- ลำโพงดิสโก้
local Colors = {{name="ฟ้า",id=1013},{name="แดง",id=331},{name="เขียว",id=1020},{name="ชมพู",id=1016},{name="เหลือง",id=1009},{name="ส้ม",id=106},{name="ม่วง",id=1015}}
local Sel1, Sel2 = 1, 2
local Speed = 0.5
local DiscoOn = false
local DiscoLoop = nil

SpeakerSection:NewDropdown({Title="สีที่ 1", Data={"ฟ้า","แดง","เขียว","ชมพู","เหลือง","ส้ม","ม่วง"}, Default="ฟ้า", Callback=function(v) for i,c in ipairs(Colors) do if c.name==v then Sel1=i break end end end})
SpeakerSection:NewDropdown({Title="สีที่ 2", Data={"ฟ้า","แดง","เขียว","ชมพู","เหลือง","ส้ม","ม่วง"}, Default="แดง", Callback=function(v) for i,c in ipairs(Colors) do if c.name==v then Sel2=i break end end end})
SpeakerSection:NewSlider({Title="ความเร็วกระพริบ", Min=0.05, Max=3, Default=0.5, Callback=function(v) Speed=v end})

SpeakerSection:NewToggle({Title="เปิด/ปิด ลำโพงดิสโก้", Default=false, Callback=function(state)
	DiscoOn = state
	if state then
		SafeNotify({Title="ลำโพงดิสโก้ ON!", Description="ทั้งเมืองต้องเต้นตาม!", Duration=5})
		DiscoLoop = task.spawn(function()
			while DiscoOn do
				pcall(function() workspace.DRadio_Script.Color:FireServer(BrickColor.new(Colors[Sel1].id)) end)
				task.wait(Speed)
				pcall(function() workspace.DRadio_Script.Color:FireServer(BrickColor.new(Colors[Sel2].id)) end)
				task.wait(Speed)
			end
		end)
	else
		if DiscoLoop then task.cancel(DiscoLoop) DiscoLoop=nil end
		SafeNotify({Title="ลำโพงปิดแล้ว", Duration=2})
	end
end})

