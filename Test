NameSection:NewToggle({Title = "ชื่อคลื่นเสียงเรนโบว์", Default = false, Callback = function(state)
	IsWaveRainbow = state
	if state then
		SafeNotify({Title="ชื่อคลื่นเสียง ON!", Description="วิ่งเป็นคลื่นสวยสุดในไทย!", Duration=5})
		WaveLoop = task.spawn(function()
			while IsWaveRainbow do
				for i=1,#waveFrames do if not IsWaveRainbow then break end pcall(function() Remote:FireServer(unpack(waveFrames[i])) end) task.wait(0.1) end
				for i=#waveFrames,1,-1 do if not IsWaveRainbow then break end pcall(function() Remote:FireServer(unpack(waveFrames[i])) end) task.wait(0.1) end
			end
		end)
	else
		if WaveLoop then task.cancel(WaveLoop) WaveLoop=nil end
		SafeNotify({Title="ชื่อคลื่นเสียง OFF", Duration=2})
	end
end})

