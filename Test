NameSection:NewToggle({Title = "ชื่อเรนจังหวะเพลง", Default = false, Callback = function(state)
	IsMusicRainbow = state
	if state then
		SafeNotify({Title="ชื่อจังหวะเพลง ON!", Description="วิ่งตามบีทสุดเท่!", Duration=5})
		MusicLoop = task.spawn(function()
			while IsMusicRainbow do
				for i=1,#musicSets do
					if not IsMusicRainbow then break end
					pcall(function() Remote:FireServer(unpack(musicSets[i])) end)
					task.wait(0.1)
				end
				for i=#musicSets,1,-1 do
					if not IsMusicRainbow then break end
					pcall(function() Remote:FireServer(unpack(musicSets[i])) end)
					task.wait(0.1)
				end
			end
		end)
	else
		if MusicLoop then task.cancel(MusicLoop) MusicLoop = nil end
		SafeNotify({Title="ชื่อจังหวะเพลง OFF", Description="หยุดวิ่งแล้ว!", Duration=2})
		pcall(function() Remote:FireServer("ชื่อปกติ", Color3.new(1,1,1), 15) end)
	end
end})

-- ====================== Settings Tab ======================
local SettingsTab = Windows:NewTab({Title = "การตั้งค่า", Description = "Rejoin + Notify + Server", Icon = "rbxassetid://7733993369"})
local SettingsSec = SettingsTab:NewSection({Title = "แจ้งเตือน & ฟีเจอร์", Position = "Left"})
local ServerSec = SettingsTab:NewSection({Title = "เซิร์ฟเวอร์", Position = "Right"})

SettingsSec:NewToggle({Title = "เปิด/ปิด Notify", Default = true, Callback = function(state) NotifyEnabled = state end})
SettingsSec:NewToggle({Title = "Auto Rejoin เมื่อ Kick", Default = true, Callback = function(state) AutoRejoinEnabled = state end})
SettingsSec:NewToggle({Title = "Auto Resume ฟาร์ม", Default = true, Callback = function(state) AutoResumeFarmEnabled = state end})

ServerSec:NewButton({Title = "ย้ายเซิร์ฟใหม่", Callback = function() TeleportService:Teleport(PlaceId, game.Players.LocalPlayer) end})
ServerSec:NewButton({Title = "รีโหลด + ย้าย", Callback = function() TeleportService:TeleportToPlaceInstance(PlaceId, game.JobId, game.Players.LocalPlayer) end})

