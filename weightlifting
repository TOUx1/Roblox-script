-- üéÆ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏¢‡∏Å‡∏ô‡πâ‡∏≥‡∏´‡∏ô‡∏±‡∏Å (‡∏¢‡∏¥‡∏á RemoteEvent)
local function autoLift()
    local tool = findTool(selectedTool)
    if not tool then
        statusLabel.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö " .. selectedTool .. "!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        toolSelectFrame.Visible = false
        return
    end

    if not character or not character:FindFirstChild("Humanoid") then
        statusLabel.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        toolSelectFrame.Visible = false
        return
    end

    if tool.Parent ~= character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            local success, err = pcall(function()
                humanoid:EquipTool(tool)
            end)
            if not success then
                statusLabel.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏ñ‡∏∑‡∏≠ " .. selectedTool .. " ‡πÑ‡∏î‡πâ!"
                statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                warn("‚ö†Ô∏è Error equipping " .. selectedTool .. ": " .. tostring(err))
                toolSelectFrame.Visible = false
                return
            end
        else
            statusLabel.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö Humanoid!"
            statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            toolSelectFrame.Visible = false
            return
        end
    end

    local muscleEvent = player:WaitForChild("muscleEvent", 5)
    if not muscleEvent then
        statusLabel.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö muscleEvent!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        warn("‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö muscleEvent ‡πÉ‡∏ô Player!")
        toolSelectFrame.Visible = false
        return
    end

    statusLabel.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏ä‡πâ " .. selectedTool .. "... üí™"
    statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)

    local args = {"rep"}
    loopConnection = RunService.Heartbeat:Connect(function()
        if isToggled and character and character:FindFirstChild("Humanoid") then
            local success, err = pcall(function()
                muscleEvent:FireServer(unpack(args))
            end)
            if not success then
                statusLabel.Text = "‚ö†Ô∏è ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏¢‡∏¥‡∏á RemoteEvent!"
                statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                warn("‚ö†Ô∏è Error firing muscleEvent: " .. tostring(err))
                loopConnection:Disconnect()
            end
            task.wait(0.1)
        else
            loopConnection:Disconnect()
            statusLabel.Text = "‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô üõë"
            statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        end
    end)
end

-- üîí ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡πá‡∏≠‡∏Ñ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
local function lockCharacter()
    if character and character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = character.HumanoidRootPart
        humanoidRootPart.Anchored = true
        lockConnection = RunService.Heartbeat:Connect(function()
            if isWarped and character and character:FindFirstChild("HumanoidRootPart") then
                humanoidRootPart.CFrame = CFrame.new(safePosition)
            else
                lockConnection:Disconnect()
                humanoidRootPart.Anchored = false
            end
        end)
    end
end

-- üåå ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÅ‡∏•‡∏∞‡∏Å‡∏•‡∏±‡∏ö
local function toggleWarp()
    if not character or not character:FindFirstChild("HumanoidRootPart") then
        statusLabel.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        return
    end

    local humanoidRootPart = character.HumanoidRootPart
    isWarped = not isWarped
    if isWarped then
        originalPosition = humanoidRootPart.Position
        humanoidRootPart.CFrame = CFrame.new(safePosition)
        warpButton.Text = "‡∏Å‡∏•‡∏±‡∏ö üîô"
        warpButton.TextColor3 = Color3.fromRGB(255, 165, 0)
        statusLabel.Text = "‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏à‡∏∏‡∏î‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢ üåå"
        statusLabel.TextColor3 = Color3.fromRGB(0, 255, 255)
        lockCharacter()
    else
        if originalPosition then
            humanoidRootPart.CFrame = CFrame.new(originalPosition)
        end
        warpButton.Text = "‡∏ß‡∏≤‡∏õ üåå"
        warpButton.TextColor3 = Color3.fromRGB(0, 255, 255)
        statusLabel.Text = "‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏î‡∏¥‡∏° üîô"
        statusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        if lockConnection then
            lockConnection:Disconnect()
            humanoidRootPart.Anchored = false
        end
    end
end

-- üèÉ‚Äç‚ôÇÔ∏è ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î
local function updateStats()
    if statLoopConnection then
        statLoopConnection:Disconnect()
    end
    statLoopConnection = RunService.Heartbeat:Connect(function()
        if character and character:FindFirstChild("Humanoid") then
            local humanoid = character.Humanoid
            local success, err = pcall(function()
                humanoid.WalkSpeed = speedValue
                humanoid.JumpPower = jumpValue
            end)
            if not success then
                statusLabel.Text = "‚ö†Ô∏è ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß/‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î!"
                statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                warn("‚ö†Ô∏è Error updating stats: " .. tostring(err))
                statLoopConnection:Disconnect()
            end
        end
    end)
end

-- ‚úçÔ∏è ‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡πà‡∏≠‡∏á‡∏Å‡∏£‡∏≠‡∏Å‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
speedInput.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local input = tonumber(speedInput.Text)
        if input and input >= 0 then
            speedValue = input
            statusLabel.Text = "‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÄ‡∏õ‡πá‡∏ô " .. input .. " üèÉ‚Äç‚ôÇÔ∏è"
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            updateStats()
        else
            statusLabel.Text = "‚ö†Ô∏è ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏Å‡∏£‡∏≠‡∏Å‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!"
            statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            speedInput.Text = tostring(speedValue)
        end
    end
end)

-- ‚úçÔ∏è ‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡πà‡∏≠‡∏á‡∏Å‡∏£‡∏≠‡∏Å‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î
jumpInput.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local input = tonumber(jumpInput.Text)
        if input and input >= 0 then
            jumpValue = input
            statusLabel.Text = "‡∏õ‡∏£‡∏±‡∏ö‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡πÄ‡∏õ‡πá‡∏ô " .. input .. " ü¶ò"
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            updateStats()
        else
            statusLabel.Text = "‚ö†Ô∏è ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏Å‡∏£‡∏≠‡∏Å‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á!"
            statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            jumpInput.Text = tostring(jumpValue)
        end
    end
end)

-- üîÑ ‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠
weightButton.MouseButton1Click:Connect(function()
    selectedTool = "Weight"
    toolSelectFrame.Visible = false
    autoLift()
end)

pushupsButton.MouseButton1Click:Connect(function()
    selectedTool = "Pushups"
    toolSelectFrame.Visible = false
    autoLift()
end)

unequipButton.MouseButton1Click:Connect(function()
    selectedTool = "Weight"
    if character and character:FindFirstChild("Humanoid") then
        local humanoid = character.Humanoid
        local success, err = pcall(function()
            humanoid:UnequipTools()
            local weightTool = findTool("Weight")
            if weightTool then
                humanoid:EquipTool(weightTool)
            end
        end)
        if not success then
            statusLabel.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏ñ‡∏∑‡∏≠ Weight ‡πÑ‡∏î‡πâ!"
            statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            warn("‚ö†Ô∏è Error equipping Weight: " .. tostring(err))
        end
    end
    toolSelectFrame.Visible = false
    autoLift()
end)

-- üîÑ ‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏õ‡∏∏‡πà‡∏°‡∏¢‡∏Å‡∏ô‡πâ‡∏≥‡∏´‡∏ô‡∏±‡∏Å
toggleButton.MouseButton1Click:Connect(function()
    isToggled = not isToggled
    if isToggled then
        toggleButton.Text = "‡∏õ‡∏¥‡∏î üõë"
        toggleButton.TextColor3 = Color3.fromRGB(255, 0, 0)
        toolSelectFrame.Visible = true -- ‡πÅ‡∏™‡∏î‡∏á UI ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠
    else
        toggleButton.Text = "‡πÄ‡∏õ‡∏¥‡∏î üöÄ"
        toggleButton.TextColor3 = Color3.fromRGB(0, 255, 0)
        if loopConnection then
            loopConnection:Disconnect()
        end
        toolSelectFrame.Visible = false
        statusLabel.Text = "‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô üõë"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        if character and character:FindFirstChild("Humanoid") then
            local humanoid = character.Humanoid
            pcall(function()
                humanoid:UnequipTools()
            end)
        end
    end
end)

-- üîÑ ‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏£‡πå‡∏õ
warpButton.MouseButton1Click:Connect(toggleWarp)

-- üîÑ ‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡πà‡∏≠‡∏á (Tabs)
farmTabButton.MouseButton1Click:Connect(function()
    farmTab.Visible = true
    playerTab.Visible = false
    farmTabButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    playerTabButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)

playerTabButton.MouseButton1Click:Connect(function()
    farmTab.Visible = false
    playerTab.Visible = true
    farmTabButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    playerTabButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
end)

-- üîÑ ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
player.CharacterAdded:Connect(function(newChar)
    character = newChar
    if isToggled then
        wait(2)
        autoLift()
    end
    if isWarped then
        wait(2)
        if character and character:FindFirstChild("HumanoidRootPart") then
            character.HumanoidRootPart.CFrame = CFrame.new(safePosition)
            lockCharacter()
        end
    end
    updateStats()
end)

-- üñ±Ô∏è ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå Hover
toggleButton.MouseEnter:Connect(function()
    toggleButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)
toggleButton.MouseLeave:Connect(function()
    toggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)

warpButton.MouseEnter:Connect(function()
    warpButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)
warpButton.MouseLeave:Connect(function()
    warpButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)

farmTabButton.MouseEnter:Connect(function()
    farmTabButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)
farmTabButton.MouseLeave:Connect(function()
    farmTabButton.BackgroundColor3 = farmTab.Visible and Color3.fromRGB(70, 70, 70) or Color3.fromRGB(50, 50, 50)
end)

playerTabButton.MouseEnter:Connect(function()
    playerTabButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)
playerTabButton.MouseLeave:Connect(function()
    playerTabButton.BackgroundColor3 = playerTab.Visible and Color3.fromRGB(70, 70, 70) or Color3.fromRGB(50, 50, 50)
end)

weightButton.MouseEnter:Connect(function()
    weightButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)
weightButton.MouseLeave:Connect(function()
    weightButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)

pushupsButton.MouseEnter:Connect(function()
    pushupsButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)
pushupsButton.MouseLeave:Connect(function()
    pushupsButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)

unequipButton.MouseEnter:Connect(function()
    unequipButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
end)
unequipButton.MouseLeave:Connect(function()
    unequipButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)

print("üéâ GUI ‡∏¢‡∏Å‡∏ô‡πâ‡∏≥‡∏´‡∏ô‡∏±‡∏Å, ‡∏ß‡∏≤‡∏£‡πå‡∏õ, ‡∏õ‡∏£‡∏±‡∏ö‡πÅ‡∏ï‡πà‡∏á, ‡πÅ‡∏•‡∏∞‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏Ñ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏°‡∏∑‡∏≠‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô! üöÄüåå")
