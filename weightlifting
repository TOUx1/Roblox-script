local toolButtons = {
    {Name = "Weight", Text = "Weight üèãÔ∏è‚Äç‚ôÇÔ∏è", Pos = UDim2.new(0.5, -80, 0, 60)},
    {Name = "Pushups", Text = "Pushups üí™", Pos = UDim2.new(0.5, -80, 0, 100)},
    {Name = "Handstands", Text = "Handstands ü§∏‚Äç‚ôÇÔ∏è", Pos = UDim2.new(0.5, -80, 0, 140)},
    {Name = "Situps", Text = "Situps üßò‚Äç‚ôÇÔ∏è", Pos = UDim2.new(0.5, -80, 0, 180)}
}
local removeButton = Instance.new("TextButton")
