local toolSelectGui = Instance.new("ScreenGui")
toolSelectGui.Name = "ToolSelectGUI"
toolSelectGui.ResetOnSpawn = false
toolSelectGui.Parent = player:WaitForChild("PlayerGui")
local toolSelectFrame = Instance.new("Frame")
