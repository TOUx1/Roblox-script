	warpTab.Visible = false
	autoTab.Visible = true
end)
--== ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏õ ==--
local warpPositions = {
	["‡∏£‡πâ‡∏≤‡∏ô‡∏õ‡∏∑‡∏ôü•∂"] = Vector3.new(495.77, 6.62, 1155.32),
	["‡∏™‡∏ñ‡∏≤‡∏ô‡∏µ‡∏ï‡∏≥‡∏£‡∏ß‡∏àüò±"] = Vector3.new(803.28, 13.27, -32.92),
	["‡∏£‡πâ‡∏≤‡∏ô‡∏£‡∏ñü§ì"] = Vector3.new(209.14, 6.43, 1176.07),
	["‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤ü§§"] = Vector3.new(767.00, 6.55, 174.00),
	["‡∏Ç‡∏ô‡∏™‡πà‡∏áü•µ"] = Vector3.new(26.80, 6.37, -193.39),
	["‡πÇ‡∏£‡∏á‡∏û‡∏¢‡∏≤‡∏ö‡∏≤‡∏•üòî"] = Vector3.new(-187.00, 6.37, 1453.00)
}

for name, pos in pairs(warpPositions) do
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(0,180,0,40)
	btn.Text = name
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 18
	btn.TextColor3 = Color3.new(1,1,1)
	btn.BackgroundColor3 = Color3.fromRGB(70,70,70)
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0,8)
	btn.Parent = warpTab

	btn.MouseButton1Click:Connect(function()
		local char = player.Character
		if char and char:FindFirstChild("HumanoidRootPart") then
			char.HumanoidRootPart.CFrame = CFrame.new(pos)
		end
	end)
end

--== ‡∏õ‡∏∏‡πà‡∏°‡∏ã‡πà‡∏≠‡∏ô/‡πÅ‡∏™‡∏î‡∏á GUI ‡∏•‡∏≤‡∏Å‡πÑ‡∏î‡πâ ==--
local hideBtn = Instance.new("TextButton")
hideBtn.Size = UDim2.new(0,40,0,40)
hideBtn.Position = UDim2.new(1,-50,0,50)
hideBtn.Text = "üëÅ"
hideBtn.Font = Enum.Font.GothamBold
hideBtn.TextSize = 18
hideBtn.TextColor3 = Color3.new(1,1,1)
hideBtn.BackgroundColor3 = Color3.fromRGB(60,60,60)
Instance.new("UICorner", hideBtn).CornerRadius = UDim.new(0,8)
hideBtn.Parent = mainFrame

local hidden = false
hideBtn.MouseButton1Click:Connect(function()
	hidden = not hidden
	contentFrame.Visible = not hidden
	sidebar.Visible = not hidden
	hideBtn.Text = hidden and "üôà" or "üëÅ"
end)

--== Dragging GUI ==--
do
	local dragging = false
	local dragStart, startPos
	mainFrame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = mainFrame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	UserInputService.InputChanged:Connect(function(input)
		if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			local delta = input.Position - dragStart
			mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
end
--== ‡∏õ‡∏∏‡πà‡∏°‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á ==--
local autoToolBtn = Instance.new("TextButton")
autoToolBtn.Size = UDim2.new(0,180,0,50)
autoToolBtn.Text = "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á: OFF"
autoToolBtn.Font = Enum.Font.GothamBold
autoToolBtn.TextSize = 18
autoToolBtn.TextColor3 = Color3.new(1,1,1)
autoToolBtn.BackgroundColor3 = Color3.fromRGB(70,70,70)
Instance.new("UICorner", autoToolBtn).CornerRadius = UDim.new(0,8)
autoToolBtn.Parent = autoTab

local autoRunning = false

local function getPunchRemote()
	local char = player.Character
	if char then
		local punchTool = char:FindFirstChild("Punch") or player.Backpack:FindFirstChild("Punch")
		if punchTool and punchTool:FindFirstChild("MeleeEvent") then
			return punchTool.MeleeEvent
		end
	end
	return nil
end

local function autoUseTool()
	while autoRunning do
		local remote = getPunchRemote()
		if remote then
			pcall(function()
				remote:FireServer()
			end)
		end
		task.wait(0.2) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤ ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡πÑ‡∏î‡πâ
	end
end

autoToolBtn.MouseButton1Click:Connect(function()
	autoRunning = not autoRunning
	if autoRunning then
		autoToolBtn.Text = "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á: ON"
		autoToolBtn.BackgroundColor3 = Color3.fromRGB(60,190,90)
		task.spawn(autoUseTool)
	else
		autoToolBtn.Text = "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á: OFF"
		autoToolBtn.BackgroundColor3 = Color3.fromRGB(70,70,70)
	end
end)

-- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ Toggle ‡πÑ‡∏°‡πà‡∏´‡∏≤‡∏¢‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡πÅ‡∏ó‡πá‡∏ö
warpTabBtn.MouseButton1Click:Connect(function()
	warpTab.Visible = true
	autoTab.Visible = false
	autoToolBtn.Text = autoRunning and "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á: ON" or "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á: OFF"
	autoToolBtn.BackgroundColor3 = autoRunning and Color3.fromRGB(60,190,90) or Color3.fromRGB(70,70,70)
end)

autoTabBtn.MouseButton1Click:Connect(function()
	warpTab.Visible = false
	autoTab.Visible = true
	autoToolBtn.Text = autoRunning and "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á: ON" or "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡∏Ç‡∏≠‡∏á: OFF"
	autoToolBtn.BackgroundColor3 = autoRunning and Color3.fromRGB(60,190,90) or Color3.fromRGB(70,70,70)
end)
