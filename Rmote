contentFrame.BackgroundTransparency = 1
contentFrame.Parent = mainFrame

-- üü¢ Tab Button Creator
local function createTabButton(tabInfo, index)
    local tabButton = Instance.new("TextButton")
    tabButton.Size = UDim2.new(1, -10, 0, 50)
    tabButton.Position = UDim2.new(0, 5, 0, (index-1) * 55 + 10)
    tabButton.BackgroundColor3 = Color3.fromRGB(35, 35, 40)
    tabButton.Text = tabInfo.name
    tabButton.TextColor3 = Color3.fromRGB(200, 200, 200)
    tabButton.TextScaled = true
    tabButton.Font = Enum.Font.Gotham
    tabButton.BorderSizePixel = 0
    tabButton.Parent = tabContainer
    
    local tabCorner = Instance.new("UICorner")
    tabCorner.CornerRadius = UDim.new(0, 8)
    tabCorner.Parent = tabButton
    
    -- Hover Effect
    tabButton.MouseEnter:Connect(function()
        if currentTab ~= index then
            TweenService:Create(tabButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(50, 50, 55)}):Play()
        end
    end)
    
    tabButton.MouseLeave:Connect(function()
        if currentTab ~= index then
            TweenService:Create(tabButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(35, 35, 40)}):Play()
        end
    end)
    
    tabButton.MouseButton1Click:Connect(function()
        switchTab(index)
    end)
    
    return tabButton
end

-- üìÑ Intercepted Tab Creator
local function createInterceptedTab()
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Name = "ScrollFrame"
    scrollFrame.Size = UDim2.new(1, -20, 1, -100)
    scrollFrame.Position = UDim2.new(0, 10, 0, 10)
    scrollFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 8
    scrollFrame.ScrollBarImageColor3 = Color3.fromRGB(100, 100, 100)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    scrollFrame.Parent = contentFrame
    
    local scrollCorner = Instance.new("UICorner")
    scrollCorner.CornerRadius = UDim.new(0, 8)
    scrollCorner.Parent = scrollFrame
    
    local header = Instance.new("TextLabel")
    header.Size = UDim2.new(1, 0, 0, 30)
    header.BackgroundTransparency = 1
    header.Text = "üì° INTERCEPTED REMOTES"
    header.TextColor3 = Color3.fromRGB(255, 255, 255)
    header.TextScaled = true
    header.Font = Enum.Font.GothamBold
    header.Parent = scrollFrame
    
    local buttonsFrame = Instance.new("Frame")
    buttonsFrame.Name = "ButtonsFrame"
    buttonsFrame.Size = UDim2.new(1, 0, 1, -30)
    buttonsFrame.Position = UDim2.new(0, 0, 0, 30)
    buttonsFrame.BackgroundTransparency = 1
    buttonsFrame.Parent = scrollFrame
    
    return scrollFrame, buttonsFrame
end

-- üîß Execute Tab Creator
local function createExecuteTab()
    local executeFrame = Instance.new("Frame")
    executeFrame.Size = UDim2.new(1, -20, 1, -20)
    executeFrame.Position = UDim2.new(0, 10, 0, 10)
    executeFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    executeFrame.BorderSizePixel = 0
    executeFrame.Parent = contentFrame
    
    local executeCorner = Instance.new("UICorner")
    executeCorner.CornerRadius = UDim.new(0, 8)
    executeCorner.Parent = executeFrame
    
    -- Code Editor
    local codeBox = Instance.new("TextBox")
    codeBox.Name = "TextBox"
    codeBox.Size = UDim2.new(1, 0, 0, 200)
    codeBox.Position = UDim2.new(0, 0, 0, 10)
    codeBox.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    codeBox.BorderSizePixel = 0
    codeBox.Text = "-- Paste intercepted remote code here\n-- Click 'RUN' to execute"
    codeBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    codeBox.TextXAlignment = Enum.TextXAlignment.Left
    codeBox.TextYAlignment = Enum.TextYAlignment.Top
    codeBox.MultiLine = true
    codeBox.TextWrapped = true
    codeBox.Font = Enum.Font.Code
    codeBox.TextSize = 14
    codeBox.ClearTextOnFocus = false
    codeBox.Parent = executeFrame
    
    local codeCorner = Instance.new("UICorner")
    codeCorner.CornerRadius = UDim.new(0, 8)
    codeCorner.Parent = codeBox
    
    -- Run Button
    local runButton = Instance.new("TextButton")
    runButton.Size = UDim2.new(0, 100, 0, 40)
    runButton.Position = UDim2.new(0, 10, 1, -50)
    runButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
    runButton.Text = "‚ñ∂Ô∏è RUN"
    runButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    runButton.TextScaled = true
    runButton.Font = Enum.Font.GothamBold
    runButton.BorderSizePixel = 0
    runButton.Parent = executeFrame
    
    local runCorner = Instance.new("UICorner")
    runCorner.CornerRadius = UDim.new(0, 8)
    runCorner.Parent = runButton
    
    -- Copy Button
    local copyButton = Instance.new("TextButton")
    copyButton.Size = UDim2.new(0, 100, 0, 40)
    copyButton.Position = UDim2.new(0, 120, 1, -50)
    copyButton.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
    copyButton.Text = "üìã COPY"
    copyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    copyButton.TextScaled = true
    copyButton.Font = Enum.Font.GothamBold
    copyButton.BorderSizePixel = 0
    copyButton.Parent = executeFrame
    
    local copyCorner = Instance.new("UICorner")
    copyCorner.CornerRadius = UDim.new(0, 8)
    copyCorner.Parent = copyButton
    
    -- Status Label
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Size = UDim2.new(1, 0, 0, 30)
    statusLabel.Position = UDim2.new(0, 0, 0, 220)
    statusLabel.BackgroundTransparency = 1
    statusLabel.Text = "Status: Ready"
    statusLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    statusLabel.TextScaled = true
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.Parent = executeFrame
    
    -- Button Functions
    runButton.MouseButton1Click:Connect(function()
        local code = codeBox.Text
        if code and code ~= "" and not string.find(code, "-- Paste intercepted remote code here") then
            statusLabel.Text = "Status: Executing..."
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            
            local success, error = pcall(function()
                loadstring(code)()
            end)
            
            if success then
                statusLabel.Text = "Status: Executed successfully! ‚úÖ"
                statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            else
                statusLabel.Text = "Status: Error - " .. tostring(error)
                statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            end
        else
            statusLabel.Text = "Status: No code to execute!"
            statusLabel.TextColor3 = Color3.fromRGB(255, 150, 0)
        end
    end)
    
    copyButton.MouseButton1Click:Connect(function()
        local code = codeBox.Text
        if code and code ~= "" then
            setclipboard(code)
            statusLabel.Text = "Status: Code copied to clipboard! üìã"
            statusLabel.TextColor3 = Color3.fromRGB(0, 200, 255)
            
            TweenService:Create(copyButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(0, 200, 100)}):Play()
            wait(0.2)
            TweenService:Create(copyButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(0, 150, 255)}):Play()
        end
    end)
    
    return executeFrame
end

-- üìä Logs Tab Creator
local function createLogsTab()
    local logsFrame = Instance.new("ScrollingFrame")
    logsFrame.Size = UDim2.new(1, -20, 1, -20)
    logsFrame.Position = UDim2.new(0, 10, 0, 10)
    logsFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
    logsFrame.BorderSizePixel = 0
    logsFrame.ScrollBarThickness = 8
    logsFrame.ScrollBarImageColor3 = Color3.fromRGB(100, 100, 100)
    logsFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    logsFrame.Parent = contentFrame
    
    local logsCorner = Instance.new("UICorner")
    logsCorner.CornerRadius = UDim.new(0, 8)
    logsCorner.Parent = logsFrame
    
    local logsLabel = Instance.new("TextLabel")
    logsLabel.Size = UDim2.new(1, 0, 0, 30)
    logsLabel.BackgroundTransparency = 1
    logsLabel.Text = "üìä INTERCEPTION LOGS"
    logsLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    logsLabel.TextScaled = true
    logsLabel.Font = Enum.Font.GothamBold
    logsLabel.Parent = logsFrame
    
    return logsFrame
end

-- üîÑ Tab Switching Function
function switchTab(tabIndex)
    -- Hide all content
    for _, tabContent in pairs(tabs) do
        if tabContent.content then
            tabContent.content.Visible = false
        end
    end
    
    -- Update tab buttons
    for i, tab in ipairs(tabs) do
        local tabButton = tabContainer:FindFirstChild("TabButton" .. i)
        if tabButton then
            if i == tabIndex then
                TweenService:Create(tabButton, TweenInfo.new(0.2), {
                    BackgroundColor3 = Color3.fromRGB(255, 50, 50),
                    TextColor3 = Color3.fromRGB(255, 255, 255)
                }):Play()
            else
                TweenService:Create(tabButton, TweenInfo.new(0.2), {
                    BackgroundColor3 = Color3.fromRGB(35, 35, 40),
                    TextColor3 = Color3.fromRGB(200, 200, 200)
                }):Play()
            end
        end
    end
    
    -- Show selected content
    if tabs[tabIndex].content then
        tabs[tabIndex].content.Visible = true
        currentTab = tabIndex
    end
end

-- üéõÔ∏è Initialize UI Tabs
for i, tabInfo in ipairs(tabs) do
    local tabButton = createTabButton(tabInfo, i)
    tabButton.Name = "TabButton" .. i
    
    if i == 1 then
        tabs[i].content, _ = createInterceptedTab()
    elseif i == 2 then
        tabs[i].content = createExecuteTab()
    elseif i == 3 then
        tabs[i].content = createLogsTab()
    end
    
    if tabs[i].content then
        tabs[i].content.Name = "TabContent" .. i
        tabs[i].content.Visible = (i == 1)
    end
end

-- Set default tab
switchTab(1)

-- üî¥ Close functionality
closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)

