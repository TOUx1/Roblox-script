-- üï∏Ô∏è Remote Interception System
local function interceptRemotes()
    print("üî• Interceptor Started! Checking for Remotes... üî•")
    
    -- Hook RemoteEvent.FireServer
    local oldFireServer = {}
    local function hookRemoteEvent(remote)
        if oldFireServer[remote] then return end
        
        print("üì° Hooked RemoteEvent: " .. remote.Name)
        oldFireServer[remote] = remote.FireServer
        remote.FireServer = function(self, ...)
            local args = {...}
            
            print(string.format("üö® Intercepted RemoteEvent: %s with %d args", remote.Name, #args))
            
            table.insert(interceptedRemotes, {
                name = remote.Name,
                args = args,
                timestamp = tick(),
                remote = remote,
                type = "RemoteEvent"
            })
            
            -- Update UI
            local interceptedTab = tabs[1].content:FindFirstChild("ScrollFrame")
            local buttonsFrame = interceptedTab and interceptedTab:FindFirstChild("ButtonsFrame")
            if interceptedTab and buttonsFrame then
                local remoteButton = Instance.new("TextButton")
                local timestamp = os.date("%H:%M:%S", math.floor(tick()))
                remoteButton.Size = UDim2.new(1, -10, 0, 60)
                remoteButton.Position = UDim2.new(0, 5, 0, #interceptedRemotes * 65)
                remoteButton.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
                remoteButton.Text = string.format("[%s] %s (%d args) [Event]", timestamp, remote.Name, #args)
                remoteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                remoteButton.TextScaled = true
                remoteButton.Font = Enum.Font.Gotham
                remoteButton.BorderSizePixel = 0
                remoteButton.Parent = buttonsFrame
                
                local buttonCorner = Instance.new("UICorner")
                buttonCorner.CornerRadius = UDim.new(0, 8)
                buttonCorner.Parent = remoteButton
                
                -- Generate Lua code
                local code = string.format("-- Intercepted: %s (RemoteEvent)\n-- Timestamp: %s\n-- Args: %d\n\n", 
                    remote.Name, timestamp, #args)
                
                for i, arg in ipairs(args) do
                    if typeof(arg) == "string" then
                        code = code .. string.format('local arg%d = "%s"\n', i, tostring(arg):gsub('"', '\\"'))
                    elseif typeof(arg) == "number" then
                        code = code .. string.format('local arg%d = %s\n', i, tostring(arg))
                    elseif typeof(arg) == "table" then
                        local success, json = pcall(function()
                            return game:GetService("HttpService"):JSONEncode(arg)
                        end)
                        code = code .. string.format('local arg%d = %s\n', i, success and json or "table")
                    else
                        code = code .. string.format('local arg%d = %s\n', i, tostring(arg))
                    end
                end
                
                code = code .. string.format('\n-- Execute with:\ngame:GetService("ReplicatedStorage"):WaitForChild("%s"):FireServer(%s)\n', 
                    remote.Name, table.concat({"arg1", "arg2", "arg3", "arg4", "arg5", "arg6", "arg7", "arg8"}, ", "))
                
                -- Click to load code
                remoteButton.MouseButton1Click:Connect(function()
                    switchTab(2)
                    local executeTab = tabs[2].content
                    local codeBox = executeTab:FindFirstChild("TextBox")
                    if codeBox then
                        codeBox.Text = code
                    end
                end)
                
                -- Update scroll size
                interceptedTab.CanvasSize = UDim2.new(0, 0, 0, #interceptedRemotes * 65 + 50)
            end
            
            -- Update logs
            local logsFrame = tabs[3].content
            if logsFrame then
                local logEntry = Instance.new("TextLabel")
                logEntry.Size = UDim2.new(1, -10, 0, 25)
                logEntry.Position = UDim2.new(0, 5, 0, logsFrame.AbsoluteCanvasSize.Y)
                logEntry.BackgroundTransparency = 1
                logEntry.Text = string.format("[%s] Intercepted %s with %d args [Event]", 
                    os.date("%H:%M:%S"), remote.Name, #args)
                logEntry.TextColor3 = Color3.fromRGB(0, 255, 0)
                logEntry.TextXAlignment = Enum.TextXAlignment.Left
                logEntry.TextSize = 12
                logEntry.Font = Enum.Font.Code
                logEntry.Parent = logsFrame
                
                logsFrame.CanvasSize = UDim2.new(0, 0, 0, logsFrame.AbsoluteCanvasSize.Y + 30)
            end
            
            -- Call original function
            return oldFireServer[remote](self, ...)
        end
    end
    
    -- Hook RemoteFunction.InvokeServer
    local oldInvokeServer = {}
    local function hookRemoteFunction(remote)
        if oldInvokeServer[remote] then return end
        
        print("üîß Hooked RemoteFunction: " .. remote.Name)
        oldInvokeServer[remote] = remote.InvokeServer
        remote.InvokeServer = function(self, ...)
            local args = {...}
            
            print(string.format("üö® Intercepted RemoteFunction: %s with %d args", remote.Name, #args))
            
            table.insert(interceptedRemotes, {
                name = remote.Name,
                args = args,
                timestamp = tick(),
                remote = remote,
                type = "RemoteFunction"
            })
            
            -- Update UI
            local interceptedTab = tabs[1].content:FindFirstChild("ScrollFrame")
            local buttonsFrame = interceptedTab and interceptedTab:FindFirstChild("ButtonsFrame")
            if interceptedTab and buttonsFrame then
                local remoteButton = Instance.new("TextButton")
                local timestamp = os.date("%H:%M:%S", math.floor(tick()))
                remoteButton.Size = UDim2.new(1, -10, 0, 60)
                remoteButton.Position = UDim2.new(0, 5, 0, #interceptedRemotes * 65)
                remoteButton.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
                remoteButton.Text = string.format("[%s] %s (%d args) [Function]", timestamp, remote.Name, #args)
                remoteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                remoteButton.TextScaled = true
                remoteButton.Font = Enum.Font.Gotham
                remoteButton.BorderSizePixel = 0
                remoteButton.Parent = buttonsFrame
                
                local buttonCorner = Instance.new("UICorner")
                buttonCorner.CornerRadius = UDim.new(0, 8)
                buttonCorner.Parent = remoteButton
                
                -- Generate Lua code
                local code = string.format("-- Intercepted: %s (RemoteFunction)\n-- Timestamp: %s\n-- Args: %d\n\n", 
                    remote.Name, timestamp, #args)
                
                for i, arg in ipairs(args) do
                    if typeof(arg) == "string" then
                        code = code .. string.format('local arg%d = "%s"\n', i, tostring(arg):gsub('"', '\\"'))
                    elseif typeof(arg) == "number" then
                        code = code .. string.format('local arg%d = %s\n', i, tostring(arg))
                    elseif typeof(arg) == "table" then
                        local success, json = pcall(function()
                            return game:GetService("HttpService"):JSONEncode(arg)
                        end)
                        code = code .. string.format('local arg%d = %s\n', i, success and json or "table")
                    else
                        code = code .. string.format('local arg%d = %s\n', i, tostring(arg))
                    end
                end
                
                code = code .. string.format('\n-- Execute with:\ngame:GetService("ReplicatedStorage"):WaitForChild("%s"):InvokeServer(%s)\n', 
                    remote.Name, table.concat({"arg1", "arg2", "arg3", "arg4", "arg5", "arg6", "arg7", "arg8"}, ", "))
                
                -- Click to load code
                remoteButton.MouseButton1Click:Connect(function()
                    switchTab(2)
                    local executeTab = tabs[2].content
                    local codeBox = executeTab:FindFirstChild("TextBox")
                    if codeBox then
                        codeBox.Text = code
                    end
                end)
                
                -- Update scroll size
                interceptedTab.CanvasSize = UDim2.new(0, 0, 0, #interceptedRemotes * 65 + 50)
            end
            
            -- Update logs
            local logsFrame = tabs[3].content
            if logsFrame then
                local logEntry = Instance.new("TextLabel")
                logEntry.Size = UDim2.new(1, -10, 0, 25)
                logEntry.Position = UDim2.new(0, 5, 0, logsFrame.AbsoluteCanvasSize.Y)
                logEntry.BackgroundTransparency = 1
                logEntry.Text = string.format("[%s] Intercepted %s with %d args [Function]", 
                    os.date("%H:%M:%S"), remote.Name, #args)
                logEntry.TextColor3 = Color3.fromRGB(0, 255, 0)
                logEntry.TextXAlignment = Enum.TextXAlignment.Left
                logEntry.TextSize = 12
                logEntry.Font = Enum.Font.Code
                logEntry.Parent = logsFrame
                
                logsFrame.CanvasSize = UDim2.new(0, 0, 0, logsFrame.AbsoluteCanvasSize.Y + 30)
            end
            
            -- Call original function
            return oldInvokeServer[remote](self, ...)
        end
    end
    
    -- üîç Hook all existing remotes
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("RemoteEvent") then
            hookRemoteEvent(obj)
        elseif obj:IsA("RemoteFunction") then
            hookRemoteFunction(obj)
        end
    end
    
    -- üîÑ Hook new remotes (dynamic)
    game.DescendantAdded:Connect(function(obj)
        if obj:IsA("RemoteEvent") then
            wait(0.1) -- Small delay for object initialization
            hookRemoteEvent(obj)
        elseif obj:IsA("RemoteFunction") then
            wait(0.1)
            hookRemoteFunction(obj)
        end
    end)
    
    print("‚úÖ Remote Interception Engine ACTIVE! üö®")
    print("üí° Tip: Try interacting with game objects to trigger remotes!")
end

-- üï∏Ô∏è Start Interception
spawn(function()
    wait(1) -- Wait for GUI to fully load
    interceptRemotes()
end)

-- üí¨ Success Notification
local notify = Instance.new("TextLabel")
notify.Size = UDim2.new(0, 300, 0, 50)
notify.Position = UDim2.new(0.5, -150, 0, 50)
notify.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
