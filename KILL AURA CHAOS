print("üíÄ AURA KILL SCRIPT v1.3 by: TOUx1 - PART 1/2 (GUI) LOADED! üíÄ")
print("üáπüá≠ ‡∏£‡∏≠ PART 2 ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô...")
--[[
    üî•üî• AURA KILL SCRIPT v1.3 by: TOUx1 - PART 2/2 üíÄüáπüá≠üî•üî•
    üíÄ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô + ‡∏õ‡∏∏‡πà‡∏°‡∏ú‡∏™‡∏° + Event Handlers üíÄ
    üìù ‡∏ß‡∏≤‡∏á‡∏ï‡πà‡∏≠‡∏à‡∏≤‡∏Å PART 1 ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ!
]]

local killCount = 0

-- üß† ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å

-- üéØ ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
local function getTargetPlayer()
    if massKillMode then
        return nil -- Mass kill mode uses all players
    end
    
    if selectedTarget and selectedTarget.Parent then
        return selectedTarget
    elseif not selectedTarget then
        -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å ‡πÉ‡∏´‡πâ‡∏´‡∏≤‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î
        local character = player.Character
        if not character or not character:FindFirstChild("HumanoidRootPart") then
            return nil
        end
        
        local myPos = character.HumanoidRootPart.Position
        local nearestPlayer = nil
        local shortestDistance = math.huge
        
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local distance = (myPos - otherPlayer.Character.HumanoidRootPart.Position).Magnitude
                if distance < shortestDistance then
                    shortestDistance = distance
                    nearestPlayer = otherPlayer
                end
            end
        end
        
        return nearestPlayer
    end
    return nil
end

-- üíÄ ‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡πÉ‡∏ô‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå
local function massKillAll()
    local successCount = 0
    local totalPlayers = 0
    
    for _, targetPlayer in pairs(Players:GetPlayers()) do
        if targetPlayer ~= player and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
            totalPlayers = totalPlayers + 1
            local success = fireRemoteAtTarget(targetPlayer)
            if success then
                successCount = successCount + 1
                killCount = killCount + 1
                killCountLabel.Text = "‡∏Ü‡πà‡∏≤: " .. killCount
            end
            wait(0.05) -- Small delay to prevent lag
        end
    end
    
    statusLabel.Text = string.format("‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô: %d/%d ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à", successCount, totalPlayers)
    statusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
    
    return successCount > 0
end

-- üéØ ‡∏¢‡∏¥‡∏á‡∏£‡∏µ‡πÇ‡∏°‡∏ó‡πÉ‡∏™‡πà‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
local function fireRemoteAtTarget(targetPlayer)
    if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("Humanoid") then
        return false
    end
    
    local success, error = pcall(function()
        local args = {
            targetPlayer.Character:WaitForChild("Humanoid"),
            false
        }
        game:GetService("ReplicatedStorage"):WaitForChild("Communication"):WaitForChild("Events"):WaitForChild(""):FireServer(unpack(args))
    end)
    
    if success then
        killCount = killCount + 1
        killCountLabel.Text = "‡∏Ü‡πà‡∏≤: " .. killCount
    end
    
    return success
end

-- üîÑ ‡∏•‡∏π‡∏õ‡∏™‡πÅ‡∏õ‡∏°
local function startSpamLoop()
    if spamConnection then
        spamConnection:Disconnect()
    end
    
    spamConnection = RunService.Heartbeat:Connect(function()
        if not isRunning then
            spamConnection:Disconnect()
            return
        end
        
        if massKillMode then
            -- Mass kill mode - target everyone
            for _, targetPlayer in pairs(Players:GetPlayers()) do
                if targetPlayer ~= player and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
                    local success = fireRemoteAtTarget(targetPlayer)
                    if success then
                        statusLabel.Text = "‡πÇ‡∏´‡∏°‡∏î‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏•‡πà‡∏≤ " .. targetPlayer.Name
                        targetDropdown.Text = "‡πÇ‡∏´‡∏°‡∏î‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô"
                        targetDropdown.TextColor3 = Color3.fromRGB(255, 0, 0)
                    end
                    wait(0.1) -- Delay between targets
                end
            end
        else
            -- Single target mode
            local targetPlayer = getTargetPlayer()
            if targetPlayer then
                local success = fireRemoteAtTarget(targetPlayer)
                if success then
                    targetDropdown.Text = targetPlayer.Name
                    targetDropdown.TextColor3 = Color3.fromRGB(255, 100, 100)
                    statusLabel.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ü‡πà‡∏≤: " .. targetPlayer.Name
                    statusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
                else
                    targetDropdown.Text = "‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î"
                    targetDropdown.TextColor3 = Color3.fromRGB(150, 150, 150)
                    statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏£‡∏µ‡πÇ‡∏°‡∏ó‡∏°‡∏µ‡∏õ‡∏±‡∏ç‡∏´‡∏≤"
                    statusLabel.TextColor3 = Color3.fromRGB(150, 0, 0)
                end
            else
                targetDropdown.Text = "‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢"
                targetDropdown.TextColor3 = Color3.fromRGB(100, 100, 100)
                statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°"
                statusLabel.TextColor3 = Color3.fromRGB(100, 100, 100)
            end
        end
    end)
end

-- üéõÔ∏è ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏∏‡πà‡∏°

-- üîÑ ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏ú‡∏™‡∏° (Toggle All)
toggleAllButton.MouseButton1Click:Connect(function()
    isRunning = not isRunning
    massKillMode = not massKillMode
    
    if isRunning and massKillMode then
        -- Start mass kill
        toggleAllButton.Text = "üíÄ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô"
        toggleAllButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
        statusLabel.Text = "‡πÇ‡∏´‡∏°‡∏î‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô: ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡πà‡∏≤!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        startSpamLoop()
        massKillAll() -- Initial mass kill
    elseif isRunning then
        -- Start single target
        toggleAllButton.Text = "üî™ ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ü‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢"
        toggleAllButton.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
        statusLabel.Text = "‡πÇ‡∏´‡∏°‡∏î‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÄ‡∏î‡∏µ‡πà‡∏¢‡∏ß: ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡πà‡∏≤!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        startSpamLoop()
    else
        -- Stop everything
        toggleAllButton.Text = "üö´ ‡∏õ‡∏¥‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î"
        toggleAllButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡∏Ü‡πà‡∏≤"
        statusLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
        if spamConnection then
            spamConnection:Disconnect()
            spamConnection = nil
        end
        massKillMode = false
    end
end)

-- üíÄ ‡∏õ‡∏∏‡πà‡∏°‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô (One-shot mass kill)
massKillButton.MouseButton1Click:Connect(function()
    massKillButton.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ü‡πà‡∏≤..."
    massKillButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    
    spawn(function()
        local success = massKillAll()
        if success then
            statusLabel.Text = "‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! üíÄ"
            statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        else
            statusLabel.Text = "‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß! ‚ö†Ô∏è"
            statusLabel.TextColor3 = Color3.fromRGB(150, 0, 0)
        end
        
        wait(2)
        massKillButton.Text = "üíÄ ‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô"
        massKillButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    end)
end)

-- ‡∏™‡∏•‡∏±‡∏ö‡πÄ‡∏£‡∏¥‡πà‡∏°/‡∏´‡∏¢‡∏∏‡∏î (Single Target Only)
toggleButton.MouseButton1Click:Connect(function()
    if massKillMode then
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏ä‡πâ‡πÇ‡∏´‡∏°‡∏î‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        return
    end
    
    if not selectedTarget and targetDropdown.Text == "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô..." then
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏Å‡πà‡∏≠‡∏ô!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 150, 0)
        return
    end
    
    isRunning = not isRunning
    
    if isRunning then
        toggleButton.Text = "‚è∏Ô∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏Ü‡πà‡∏≤"
        toggleButton.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
        statusLabel.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ü‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÄ‡∏î‡∏µ‡πà‡∏¢‡∏ß"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        startSpamLoop()
    else
        toggleButton.Text = "‚ñ∂Ô∏è ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Ü‡πà‡∏≤"
        toggleButton.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡∏Ü‡πà‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢"
        statusLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
        if spamConnection then
            spamConnection:Disconnect()
            spamConnection = nil
        end
    end
end)

-- ‡∏´‡∏¢‡∏∏‡∏î‡∏â‡∏∏‡∏Å‡πÄ‡∏â‡∏¥‡∏ô
stopButton.MouseButton1Click:Connect(function()
    isRunning = false
    massKillMode = false
    if spamConnection then
        spamConnection:Disconnect()
        spamConnection = nil
    end
    
    toggleButton.Text = "‚ñ∂Ô∏è ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Ü‡πà‡∏≤"
    toggleButton.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
    toggleAllButton.Text = "üö´ ‡∏õ‡∏¥‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î"
    toggleAllButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    massKillButton.Text = "üíÄ ‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô"
    massKillButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    
    statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á"
    statusLabel.TextColor3 = Color3.fromRGB(150, 150, 150)
    targetDropdown.Text = selectedTarget and selectedTarget.Name or "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô..."
    targetDropdown.TextColor3 = selectedTarget and Color3.fromRGB(255, 100, 100) or Color3.fromRGB(200, 200, 200)
end)

-- üéØ ‡∏ã‡πà‡∏≠‡∏ô/‡πÅ‡∏™‡∏î‡∏á
minimizeButton.MouseButton1Click:Connect(function()
    guiVisible = not guiVisible
    
    local contentToHide = {targetFrame, statusFrame, controlsFrame, settingsFrame}
    
    if guiVisible then
        -- ‡πÅ‡∏™‡∏î‡∏á GUI
        TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 400, 0, 340)
        }):Play()
        minimizeButton.Text = "‚àí"
        minimizeButton.BackgroundColor3 = Color3.fromRGB(150, 150, 0)
        
        -- ‡πÅ‡∏™‡∏î‡∏á‡πÄ‡∏ô‡∏∑‡πâ‡∏≠‡∏´‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
        for _, frame in pairs(contentToHide) do
            frame.Visible = true
        end
    else
        -- ‡∏ã‡πà‡∏≠‡∏ô GUI
        TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Size = UDim2.new(0, 400, 0, 45)
        }):Play()
        minimizeButton.Text = "+"
        minimizeButton.BackgroundColor3 = Color3.fromRGB(150, 100, 0)
        
        -- ‡∏ã‡πà‡∏≠‡∏ô‡πÄ‡∏ô‡∏∑‡πâ‡∏≠‡∏´‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
        for _, frame in pairs(contentToHide) do
            frame.Visible = false
        end
    end
end)

-- ‡∏õ‡∏¥‡∏î GUI
closeButton.MouseButton1Click:Connect(function()
    isRunning = false
    massKillMode = false
    if spamConnection then
        spamConnection:Disconnect()
    end
    screenGui:Destroy()
end)

-- üéØ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏°‡∏ô‡∏π‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local dropdownOpen = false
local dropdownList = nil

targetDropdown.MouseButton1Click:Connect(function()
    if massKillMode then
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏ä‡πâ‡πÇ‡∏´‡∏°‡∏î‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        return
    end
    
    if dropdownOpen then
        if dropdownList then
            dropdownList:Destroy()
        end
        dropdownOpen = false
        return
    end
    
    dropdownOpen = true
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
    dropdownList = Instance.new("Frame")
    dropdownList.Size = UDim2.new(0, 220, 0, 0)
    dropdownList.Position = UDim2.new(0, targetDropdown.AbsolutePosition.X - mainFrame.AbsolutePosition.X + 10, 0, targetDropdown.AbsolutePosition.Y + targetDropdown.AbsoluteSize.Y - mainFrame.AbsolutePosition.Y + 5)
    dropdownList.BackgroundColor3 = Color3.fromRGB(25, 15, 15)
    dropdownList.BorderSizePixel = 0
    dropdownList.Parent = mainFrame
    
    local listCorner = Instance.new("UICorner")
    listCorner.CornerRadius = UDim.new(0, 8)
    listCorner.Parent = dropdownList
    
    local listLayout = Instance.new("UIListLayout")
    listLayout.SortOrder = Enum.SortOrder.LayoutOrder
    listLayout.Padding = UDim.new(0, 2)
    listLayout.Parent = dropdownList
    
    -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÉ‡∏ô‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£
    local playerCount = 0
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player then
            playerCount = playerCount + 1
            local playerButton = Instance.new("TextButton")
            playerButton.Size = UDim2.new(1, -10, 0, 30)
            playerButton.BackgroundColor3 = Color3.fromRGB(35, 20, 20)
            playerButton.Text = "üéØ " .. otherPlayer.Name
            playerButton.TextColor3 = Color3.fromRGB(255, 200, 200)
            playerButton.TextScaled = true
            playerButton.Font = Enum.Font.Gotham
            playerButton.BorderSizePixel = 0
            playerButton.LayoutOrder = playerCount
            playerButton.Parent = dropdownList
            
            local playerCorner = Instance.new("UICorner")
            playerCorner.CornerRadius = UDim.new(0, 6)
            playerCorner.Parent = playerButton
            
            playerButton.MouseButton1Click:Connect(function()
                selectedTarget = otherPlayer
                targetDropdown.Text = otherPlayer.Name
                targetDropdown.TextColor3 = Color3.fromRGB(255, 100, 100)
                if dropdownList then
                    dropdownList:Destroy()
                end
                dropdownOpen = false
                statusLabel.Text = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏Ü‡πà‡∏≤: " .. otherPlayer.Name
                statusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
            end)
            
            playerButton.MouseEnter:Connect(function()
                TweenService:Create(playerButton, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(50, 30, 30)}):Play()
            end)
            
            playerButton.MouseLeave:Connect(function()
                TweenService:Create(playerButton, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(35, 20, 20)}):Play()
            end)
        end
    end
    
    dropdownList.Size = UDim2.new(0, 220, 0, math.max(playerCount * 32, 32))
    
    -- ‡∏õ‡∏¥‡∏î‡πÄ‡∏°‡∏ô‡∏π‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ô‡∏≠‡∏Å
    local connection
    connection = UserInputService.InputBegan:Connect(function(input)
        if dropdownOpen and input.UserInputType == Enum.UserInputType.MouseButton1 then
            local mousePos = UserInputService:GetMouseLocation()
            local dropdownPos = dropdownList.AbsolutePosition
            local dropdownSize = dropdownList.AbsoluteSize
            
            if mousePos.X < dropdownPos.X or mousePos.X > dropdownPos.X + dropdownSize.X or
               mousePos.Y < dropdownPos.Y or mousePos.Y > dropdownPos.Y + dropdownSize.Y then
                if dropdownList then
                    dropdownList:Destroy()
                end
                dropdownOpen = false
                connection:Disconnect()
            end
        end
    end)
end)

-- üéöÔ∏è ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡πÑ‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå
sliderHandle.MouseButton1Down:Connect(function()
    draggingSlider = true
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        draggingSlider = false
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if draggingSlider and input.UserInputType == Enum.UserInputType.MouseMovement then
        local mouse = player:GetMouse()
        local sliderPos = rateSlider.AbsolutePosition
        local sliderSize = rateSlider.AbsoluteSize
        
        local relativeX = math.clamp((mouse.X - sliderPos.X) / sliderSize.X, 0, 1)
        sliderHandle.Position = UDim2.new(relativeX, -6, 0, 0)
        
        -- ‡πÅ‡∏õ‡∏•‡∏á‡πÄ‡∏õ‡πá‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏™‡πÅ‡∏õ‡∏° (0.1 ‡∏ñ‡∏∂‡∏á 2.0 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ)
        spamRate = 0.1 + (relativeX * 1.9)
        rateValueLabel.Text = string.format("%.1f‡∏ß‡∏¥‡∏ô.", spamRate)
        rateValueLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

-- üíÄ ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡∏ï‡πâ‡∏≠‡∏ô‡∏£‡∏±‡∏ö (TOUx1 Edition)
local notify = Instance.new("TextLabel")
notify.Size = UDim2.new(0, 350, 0, 50)
notify.Position = UDim2.new(0.5, -175, 0, 20)
notify.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
notify.Text = "üíÄ ‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤ script by: TOUx1 v1.3 ‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß! üíÄ"
notify.TextColor3 = Color3.fromRGB(255, 255, 255)
notify.TextScaled = true
notify.Font = Enum.Font.GothamBold
notify.Parent = screenGui

local notifyCorner = Instance.new("UICorner")
notifyCorner.CornerRadius = UDim.new(0, 10)
notifyCorner.Parent = notify

TweenService:Create(notify, TweenInfo.new(0.6), {Position = UDim2.new(1, 25, 0, 20)}):Play()
game:GetService("Debris"):AddItem(notify, 5)

