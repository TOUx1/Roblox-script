--[[
    üî•üî• by:TOU | CHAOS KILL AURA v1.4 - PART 2/2 üáπüá≠üî•üî•
    üíÄ ‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î‡πÇ‡∏´‡∏°‡∏î‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô + TextBox Remote + ‡∏î‡∏µ‡∏ö‡∏±‡πä‡∏Å üíÄ
    üìù ‡∏ß‡∏≤‡∏á‡∏ï‡πà‡∏≠‡∏à‡∏≤‡∏Å PART 1/2 (v1.4) ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ!
]]

-- üß† ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°
local remoteToUse = nil
local remoteScanComplete = false

-- üîç ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡πÅ‡∏Å‡∏ô Remote Events
local function scanRemotes()
    local foundRemotes = {}
    local eventsFolder = ReplicatedStorage:FindFirstChild("Communication") and
                        ReplicatedStorage.Communication:FindFirstChild("Events")
    
    if eventsFolder then
        for _, obj in pairs(eventsFolder:GetChildren()) do
            if obj:IsA("RemoteEvent") then
                table.insert(foundRemotes, {Name = obj.Name, Path = "ReplicatedStorage.Communication.Events." .. obj.Name})
            end
        end
    end
    
    if #foundRemotes > 0 then
        remoteToUse = foundRemotes[1].Name
        statusLabel.Text = "‡∏û‡∏ö Remote: " .. remoteToUse
        statusLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
        remoteInput.Text = remoteToUse -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï TextBox
        print("üîç ‡∏û‡∏ö Remote Events ‡πÉ‡∏ô Communication.Events: ")
        for _, remote in pairs(foundRemotes) do
            print(" - " .. remote.Path)
        end
    else
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡πÑ‡∏°‡πà‡∏û‡∏ö Remote Event ‡πÉ‡∏ô Communication.Events!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        remoteInput.Text = "Auto"
        print("üö® ‡πÑ‡∏°‡πà‡∏û‡∏ö Remote Event ‡πÉ‡∏ô ReplicatedStorage.Communication.Events!")
    end
    remoteScanComplete = true
end

-- üéØ ‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
local function getTargetPlayer()
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
        return nil, "‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏°‡πà‡∏û‡∏£‡πâ‡∏≠‡∏°"
    end
    
    if isKillAll then
        local myPos = player.Character.HumanoidRootPart.Position
        local nearestPlayer = nil
        local shortestDistance = math.huge
        
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local humanoid = otherPlayer.Character:FindFirstChild("Humanoid")
                local hasForceField = otherPlayer.Character:FindFirstChild("ForceField")
                if humanoid and humanoid.Health > 0 and not hasForceField then -- üÜï ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ï‡∏≤‡∏¢/‡πÇ‡∏•‡πà
                    local distance = (myPos - otherPlayer.Character.HumanoidRootPart.Position).Magnitude
                    if distance < shortestDistance then
                        shortestDistance = distance
                        nearestPlayer = otherPlayer
                    end
                end
            end
        end
        return nearestPlayer, nearestPlayer and "‡πÇ‡∏´‡∏°‡∏î‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô" or "‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°"
    elseif selectedTarget and selectedTarget.Parent then
        local humanoid = selectedTarget.Character and selectedTarget.Character:FindFirstChild("Humanoid")
        local hasForceField = selectedTarget.Character and selectedTarget.Character:FindFirstChild("ForceField")
        if humanoid and humanoid.Health > 0 and not hasForceField then
            return selectedTarget, "‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å"
        else
            return nil, "‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ï‡∏≤‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡∏°‡∏µ‡πÇ‡∏•‡πà"
        end
    end
    return nil, "‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢"
end

-- üéØ ‡∏¢‡∏¥‡∏á‡∏£‡∏µ‡πÇ‡∏°‡∏ó‡πÉ‡∏™‡πà‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
local function fireRemoteAtTarget(targetPlayer)
    if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("Humanoid") then
        return false, "‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÑ‡∏°‡πà‡∏°‡∏µ Humanoid"
    end
    
    if not remoteToUse or not remoteScanComplete then
        return false, "‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏û‡∏ö Remote"
    end
    
    local success, errorMsg = pcall(function()
        local remote = ReplicatedStorage.Communication.Events:FindFirstChild(remoteToUse)
        if not remote then
            error("‡πÑ‡∏°‡πà‡∏û‡∏ö Remote: " .. remoteToUse)
        end
        
        local args = {
            targetPlayer.Character.Humanoid,
            false
        }
        remote:FireServer(unpack(args))
    end)
    
    if success then
        print("üî• ‡∏¢‡∏¥‡∏á‡∏£‡∏µ‡πÇ‡∏°‡∏ó‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡∏ó‡∏µ‡πà " .. targetPlayer.Name .. " ‡∏î‡πâ‡∏ß‡∏¢ Remote: " .. remoteToUse)
    else
        print("üö® ‡∏Å‡∏≤‡∏£‡∏¢‡∏¥‡∏á‡∏£‡∏µ‡πÇ‡∏°‡∏ó‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß: " .. tostring(errorMsg))
    end
    return success, errorMsg
end

-- üîÑ ‡∏•‡∏π‡∏õ‡∏™‡πÅ‡∏õ‡∏°
local function startSpamLoop()
    if spamConnection then
        spamConnection:Disconnect()
        spamConnection = nil
    end
    
    spamConnection = RunService.Heartbeat:Connect(function()
        if not isRunning then
            spamConnection:Disconnect()
            spamConnection = nil
            return
        end
        
        local targetPlayer, targetStatus = getTargetPlayer()
        if targetPlayer then
            local success, errorMsg = fireRemoteAtTarget(targetPlayer)
            if success then
                targetDropdown.Text = isKillAll and "‡πÇ‡∏´‡∏°‡∏î: ‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô (" .. targetPlayer.Name .. ")" or targetPlayer.Name
                targetDropdown.TextColor3 = Color3.fromRGB(0, 255, 150)
                statusLabel.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏¢‡∏¥‡∏á: " .. targetPlayer.Name
                statusLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
            else
                targetDropdown.Text = "‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î"
                targetDropdown.TextColor3 = Color3.fromRGB(255, 100, 100)
                statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: " .. (errorMsg or "‡∏£‡∏µ‡πÇ‡∏°‡∏ó‡∏°‡∏µ‡∏õ‡∏±‡∏ç‡∏´‡∏≤")
                statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            end
        else
            targetDropdown.Text = isKillAll and "‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡πÄ‡∏´‡∏°‡∏≤‡∏∞‡∏™‡∏°" or "‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢"
            targetDropdown.TextColor3 = Color3.fromRGB(255, 150, 0)
            statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: " .. targetStatus
            statusLabel.TextColor3 = Color3.fromRGB(255, 150, 0)
        end
        
        wait(spamRate + math.random(0.01, 0.05))
    end)
end

-- üéõÔ∏è ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏∏‡πà‡∏°

-- ‡∏™‡∏•‡∏±‡∏ö‡πÄ‡∏£‡∏¥‡πà‡∏°/‡∏´‡∏¢‡∏∏‡∏î
toggleButton.MouseButton1Click:Connect(function()
    if not isKillAll and not selectedTarget and targetDropdown.Text == "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô..." then
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏Å‡πà‡∏≠‡∏ô!"
        statusLabel.TextColor3 = Color3.fromRGB(255, 150, 0)
        return
    end
    
    isRunning = not isRunning
    isKillAll = false
    
    if isRunning then
        toggleButton.Text = "‚è∏Ô∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß"
        toggleButton.BackgroundColor3 = Color3.fromRGB(255, 200, 0)
        killAllButton.Text = "üí• ‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô"
        killAllButton.BackgroundColor3 = Color3.fromRGB(200, 0, 200)
        startSpamLoop()
    else
        toggleButton.Text = "‚ñ∂Ô∏è ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏≠‡∏£‡πà‡∏≤"
        toggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡∏ä‡∏±‡πà‡∏ß‡∏Ñ‡∏£‡∏≤‡∏ß"
        statusLabel.TextColor3 = Color3.fromRGB(255, 200, 0)
        if spamConnection then
            spamConnection:Disconnect()
            spamConnection = nil
        end
    end
end)

-- ‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô
killAllButton.MouseButton1Click:Connect(function()
    isRunning = not isRunning
    isKillAll = isRunning
    
    if isRunning then
        killAllButton.Text = "‚è∏Ô∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤"
        killAllButton.BackgroundColor3 = Color3.fromRGB(255, 100, 255)
        toggleButton.Text = "‚ñ∂Ô∏è ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏≠‡∏≠‡∏£‡πà‡∏≤"
        toggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        startSpamLoop()
    else
        killAllButton.Text = "üí• ‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô"
        killAllButton.BackgroundColor3 = Color3.fromRGB(200, 0, 200)
        statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡πÇ‡∏´‡∏°‡∏î‡∏≠‡πà‡∏≠‡∏£‡πà‡∏≤‡∏Ü‡πà‡∏≤"
