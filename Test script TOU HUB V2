local URL_BOX = "https://pastefy.app/VaHDLWjU/raw"
local URL_MED = "https://pastefy.app/ji38FSCI/raw"
local URL_PREMIUM = "https://raw.githubusercontent.com/TOUx1/VIP-FARM-CT2/main/VIP.lua" -- ‡∏¢‡∏±‡∏á‡πÉ‡∏ä‡πâ‡πÄ‡∏î‡∏¥‡∏° ‡πÅ‡∏ï‡πà‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ä‡∏∑‡πà‡∏≠

HouseSec:NewButton({Title="‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏Ç‡∏≤‡∏¢‡∏ö‡πâ‡∏≤‡∏ô üè†üí•", Callback=function() SilentLoad("https://pastefy.app/8V7h6QG3/raw") end})

_G.SellToggle = FarmL:NewToggle({Title="Auto Farm üí∏", Default=false, Callback=function(s)
    FarmStates.Sell=s
    if s then SilentLoad(URL_SELL) _G.FarmLoop=game:GetService("RunService").Heartbeat:Connect(function() if not FarmStates.Sell then return end SilentLoad(URL_SELL) task.wait(0.05) end)
    else if _G.FarmLoop then _G.FarmLoop:Disconnect() _G.FarmLoop=nil end end
end})

_G.BoxToggle = FarmL:NewToggle({Title="‡∏™‡πà‡∏á‡∏Å‡∏•‡πà‡∏≠‡∏á üì¶", Default=false, Callback=function(s)
    FarmStates.Box=s
    if s then SilentLoad(URL_BOX) task.spawn(function() while FarmStates.Box do task.wait(15) SilentLoad(URL_BOX) end end) end
end})

_G.MedToggle = FarmL:NewToggle({Title="‡∏à‡πà‡∏≤‡∏¢‡∏¢‡∏≤ üíâ", Default=false, Callback=function(s)
    FarmStates.Med=s
    if s then SilentLoad(URL_MED) task.spawn(function() while FarmStates.Med do task.wait(1) SilentLoad(URL_MED) end end) end
end})

