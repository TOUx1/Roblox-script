TS.TeleportInitFailed:Connect(Rejoin)
game.Players.LocalPlayer.OnTeleport:Connect(function(s) if s==Enum.TeleportState.Failed then Rejoin() end end)

game.Players.LocalPlayer.CharacterAdded:Connect(function()
