-- TOU HUB v34 ULTIMATE ‚Äî ‡∏Ñ‡∏£‡∏ö‡∏ó‡∏∏‡∏Å‡∏≠‡∏¢‡πà‡∏≤‡∏á 100% üî• (‡∏™‡πà‡∏ß‡∏ô‡∏ó‡∏µ‡πà 1/2)

local NothingLibrary = loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/3345-c-a-t-s-u-s/NOTHING/main/source.lua'))()
local Notify = NothingLibrary.Notification()

local NotifyEnabled = true
local AutoRejoinEnabled = true
local AutoResumeFarmEnabled = true

local FarmStates = { Sell = false, Box = false, Medicine = false, VIP = false }
_G.SellToggle = nil _G.BoxToggle = nil _G.MedToggle = nil _G.VIPToggle = nil
_G.FarmLoop = nil _G.VIPLoop = nil

local IsWaveRainbow = false
local IsMusicRainbow = false
local WaveLoop = nil
local MusicLoop = nil
local DiscoOn = false
local DiscoLoop = nil

local Remote = game:GetService("ReplicatedStorage"):FindFirstChild("NameTagRemoteEvent")

local function SafeNotify(d) if NotifyEnabled then Notify.new(d) end end

SafeNotify({Title="TOU HUB v34 ULTIMATE üî•", Description="‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÇ‡∏´‡∏•‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î... ‡πÉ‡∏à‡πÄ‡∏¢‡πá‡∏ô‡∏ô‡∏∞‡∏ö‡∏≠‡∏™ üòà", Duration=8})

local Windows = NothingLibrary.new({
	Title = "TOU HUB v34 ULTIMATE üíú",
	Description = "City Thailand 2 ‚Äî ‡∏Ñ‡∏£‡∏ö‡∏ó‡∏∏‡∏Å‡∏ü‡∏µ‡πÄ‡∏à‡∏≠‡∏£‡πå + VIP FARM üëë",
	Keybind = Enum.KeyCode.LeftControl,
	Logo = 'http://www.roblox.com/asset/?id=18898582662'
})

local function GetHumanoid() local plr = game.Players.LocalPlayer local char = plr.Character or plr.CharacterAdded:Wait() return char:WaitForChild("Humanoid",10) end
local function GetRootPart() local plr = game.Players.LocalPlayer local char = plr.Character or plr.CharacterAdded:Wait() return char:WaitForChild("HumanoidRootPart",10) end

local TeleportService = game:GetService("TeleportService")
local PlaceId = game.PlaceId

local function Rejoin() if AutoRejoinEnabled then task.wait(5) TeleportService:Teleport(PlaceId, game.Players.LocalPlayer) end end
game:GetService("GuiService").ErrorMessageChanged:Connect(Rejoin)
TeleportService.TeleportInitFailed:Connect(Rejoin)
game.Players.LocalPlayer.OnTeleport:Connect(function(s) if s == Enum.TeleportState.Failed then Rejoin() end end)

game.Players.LocalPlayer.CharacterAdded:Connect(function()
	if AutoResumeFarmEnabled then task.wait(3)
		if FarmStates.Sell and _G.SellToggle then _G.SellToggle:Callback(true) end
		if FarmStates.Box and _G.BoxToggle then _G.BoxToggle:Callback(true) end
		if FarmStates.Medicine and _G.MedToggle then _G.MedToggle:Callback(true) end
		if FarmStates.VIP and _G.VIPToggle then _G.VIPToggle:Callback(true) end
		SafeNotify({Title="‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡πÅ‡∏•‡πâ‡∏ß‡∏ö‡∏≠‡∏™! üî•", Description="‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏£‡∏±‡∏ô‡∏ï‡πà‡∏≠‡∏ó‡∏±‡∏ô‡∏ó‡∏µ üí∏", Duration=4})
	end
end)

local function SilentLoad(url) task.spawn(function() pcall(function() loadstring(game:HttpGet(url))() end) end) end

-- ====================== FARM TAB ======================
local FarmTab = Windows:NewTab({Title="‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏á‡∏¥‡∏ô üí∞", Icon="rbxassetid://7733960981"})
local FarmLeft = FarmTab:NewSection({Title="‡∏≠‡πâ‡∏≠‡πÇ‡∏ï‡πâ‡∏ü‡∏≤‡∏£‡πå‡∏°‡πÄ‡∏á‡∏¥‡∏ô üöÄ", Position="Left"})
local AutoHouseSection = FarmTab:NewSection({Title="‡∏≠‡πâ‡∏≠‡πÇ‡∏ï‡πâ‡∏Ç‡∏≤‡∏¢‡∏ö‡πâ‡∏≤‡∏ô üè†", Position="Right"})
local FarmRight = FarmTab:NewSection({Title="‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ ‚öôÔ∏è", Position="Right"})
local FarmInfo = FarmTab:NewSection({Title="Info üìå", Position="Right"})

local URL_FARM = "https://raw.githubusercontent.com/TOUx1/Roblox-script/refs/heads/main/Sell"
local URL_BOX  = "https://pastefy.app/VaHDLWjU/raw"
local URL_MED  = "https://pastefy.app/ji38FSCI/raw"

AutoHouseSection:NewButton({Title="‡∏≠‡πâ‡∏≠‡πÇ‡∏ï‡πâ‡∏Ç‡∏≤‡∏¢‡∏ö‡πâ‡∏≤‡∏ô üè†üí•", Callback=function()
	SafeNotify({Title="‡∏Ç‡∏≤‡∏¢‡∏ö‡πâ‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß!", Description="‡πÄ‡∏á‡∏¥‡∏ô‡∏°‡∏≤‡πÅ‡∏ô‡πà üåä", Duration=5})
	loadstring(game:HttpGet("https://pastefy.app/8V7h6QG3/raw"))()
end})

_G.SellToggle = FarmLeft:NewToggle({Title="Auto Farm (‡∏ã‡∏∑‡πâ‡∏≠‡∏ö‡πâ‡∏≤‡∏ô‡∏Å‡πà‡∏≠‡∏ô) üí∏", Default=false, Callback=function(s)
	FarmStates.Sell = s
	if s then SilentLoad(URL_FARM) _G.FarmLoop = game:GetService("RunService").Heartbeat:Connect(function() if not FarmStates.Sell then return end SilentLoad(URL_FARM) task.wait(0.05) end)
	else if _G.FarmLoop then _G.FarmLoop:Disconnect() _G.FarmLoop=nil end end
end})

_G.BoxToggle = FarmLeft:NewToggle({Title="‡∏≠‡πâ‡∏≠‡πÇ‡∏ï‡πâ‡∏™‡πà‡∏á‡∏Å‡∏•‡πà‡∏≠‡∏á üì¶", Default=false, Callback=function(s)
	FarmStates.Box = s
	if s then SilentLoad(URL_BOX) task.spawn(function() while FarmStates.Box do task.wait(15) if not FarmStates.Box then break end SilentLoad(URL_BOX) end end) end
end})

_G.MedToggle = FarmLeft:NewToggle({Title="‡∏≠‡πâ‡∏≠‡πÇ‡∏ï‡πâ‡∏à‡πà‡∏≤‡∏¢‡∏¢‡∏≤ üíâ", Default=false, Callback=function(s)
	FarmStates.Medicine = s
	if s then SilentLoad(URL_MED) task.spawn(function() while FarmStates.Medicine do task.wait(1) if not FarmStates.Medicine then break end SilentLoad(URL_MED) end end) end
end})

-- ====================== VIP FARM TAB ======================
