local function TP(pos,name)
    local root = GetRootPart()
    if root then
        for _,p in pairs(root.Parent:GetDescendants()) do if p:IsA("BasePart") and p~=root then p.CanCollide=false end end
        task.wait(0.1)
        root.CFrame = CFrame.new(pos)
        task.wait(0.4)
        for _,p in pairs(root.Parent:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide=true end end
        SafeNotify({Title="‡∏ß‡∏≤‡∏õ‡πÅ‡∏•‡πâ‡∏ß! ‚ú®", Description="‡πÑ‡∏õ "..name.." ‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢ üî•", Duration=2})
    end
end

