-- ====================== WARP TAB ======================
local WarpTab = Windows:NewTab({Title="‡∏ß‡∏≤‡∏õ‡∏î‡πà‡∏ß‡∏ô 11 ‡∏à‡∏∏‡∏î üöÄ", Icon="rbxassetid://7743866277"})
local WarpSection = WarpTab:NewSection({Title="‡∏ß‡∏≤‡∏õ‡∏à‡∏∏‡∏î‡∏™‡∏≥‡∏Ñ‡∏±‡∏ç ‚ö°", Position="Left"})

local function TP(pos,name)
	local root = GetRootPart()
	if root then
		for _,p in pairs(root.Parent:GetDescendants()) do if p:IsA("BasePart") and p~=root then p.CanCollide=false end end
		task.wait(0.1)
		root.CFrame = CFrame.new(pos)
		task.wait(0.4)
		for _,p in pairs(root.Parent:GetDescendants()) do if p:IsA("BasePart") then p.CanCollide=true end end
		SafeNotify({Title="‡∏ß‡∏≤‡∏õ‡πÅ‡∏•‡πâ‡∏ß! ‚ú®", Description="‡πÑ‡∏õ "..name, Duration=2})
	end
end

local warps = {
