    if s then
        SafeNotify({Title="Premium Farm ON! ü§ë", Description="‡πÄ‡∏á‡∏¥‡∏ô‡πÑ‡∏´‡∏•‡πÄ‡∏á‡∏µ‡∏¢‡∏ö‡∏Å‡∏£‡∏¥‡∏ö ‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÉ‡∏Ñ‡∏£‡∏£‡∏π‡πâ üî•", Duration=7})
        SilentLoad(URL_PREMIUM)
        _G.PremiumLoop=game:GetService("RunService").Heartbeat:Connect(function()
            if not FarmStates.Premium then return end
            SilentLoad(URL_PREMIUM)
