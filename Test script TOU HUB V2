-- TOU HUB v34 ULTIMATE ‚Äî ‡πÅ‡∏Å‡πâ‡∏ï‡∏≤‡∏°‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏ö‡∏≠‡∏™‡πÉ‡∏´‡∏ç‡πà (‡πÑ‡∏°‡πà‡∏°‡∏µ VIP ‡πÅ‡∏•‡πâ‡∏ß + ‡∏°‡∏µ‡∏ä‡∏∑‡πà‡∏≠‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å) üî•

local NothingLibrary = loadstring(game:HttpGet('https://raw.githubusercontent.com/3345-c-a-t-s-u-s/NOTHING/main/source.lua'))()
local Notify = NothingLibrary.Notification()

local NotifyEnabled = true
local AutoRejoinEnabled = true
local AutoResumeFarmEnabled = true

local FarmStates = {Sell=false, Box=false, Med=false, Premium=false}
_G.SellToggle, _G.BoxToggle, _G.MedToggle, _G.PremiumToggle = nil,nil,nil,nil
_G.FarmLoop, _G.PremiumLoop = nil,nil

local IsWaveRainbow = false
local IsMusicRainbow = false
local WaveLoop, MusicLoop = nil,nil
local DiscoOn = false
local DiscoLoop = nil

local Remote = game:GetService("ReplicatedStorage"):FindFirstChild("NameTagRemoteEvent")

local function SafeNotify(d) if NotifyEnabled then Notify.new(d) end end
SafeNotify({Title="TOU HUB v34 ULTIMATE üî•", Description="‡∏ö‡∏≠‡∏™‡∏™‡∏±‡πà‡∏á‡∏°‡∏≤ ‡∏Å‡∏π‡∏à‡∏±‡∏î‡πÉ‡∏´‡πâ‡∏Ñ‡∏£‡∏ö ‡πÑ‡∏°‡πà‡∏°‡∏µ VIP ‡πÅ‡∏•‡πâ‡∏ß üñ§", Duration=8})

local Windows = NothingLibrary.new({
    Title = "TOU HUB v34 ULTIMATE üíú",
    Description = "City Thailand 2 ‚Äî Premium Farm + ‡∏ä‡∏∑‡πà‡∏≠‡∏Å‡∏£‡∏∞‡∏ï‡∏∏‡∏Å + FE ‡∏î‡∏∏‡πÄ‡∏î‡∏∑‡∏≠‡∏î",
    Keybind = Enum.KeyCode.LeftControl,
    Logo = 'http://www.roblox.com/asset/?id=18898582662'
})

local function GetHumanoid() local c=game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait() return c:WaitForChild("Humanoid",10) end
local function GetRootPart() local c=game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait() return c:WaitForChild("HumanoidRootPart",10) end

local TS = game:GetService("TeleportService")
local PlaceId = game.PlaceId

local function Rejoin() if AutoRejoinEnabled then task.wait(5) TS:Teleport(PlaceId, game.Players.LocalPlayer) end end
game:GetService("GuiService").ErrorMessageChanged:Connect(Rejoin)
TS.TeleportInitFailed:Connect(Rejoin)
game.Players.LocalPlayer.OnTeleport:Connect(function(s) if s==Enum.TeleportState.Failed then Rejoin() end end)

game.Players.LocalPlayer.CharacterAdded:Connect(function()
    if AutoResumeFarmEnabled then task.wait(3)
        if FarmStates.Sell and _G.SellToggle then _G.SellToggle:Callback(true) end
        if FarmStates.Box and _G.BoxToggle then _G.BoxToggle:Callback(true) end
        if FarmStates.Med and _G.MedToggle then _G.MedToggle:Callback(true) end
        if FarmStates.Premium and _G.PremiumToggle then _G.PremiumToggle:Callback(true) end
        SafeNotify({Title="‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡πÅ‡∏•‡πâ‡∏ß‡∏ö‡∏≠‡∏™! ü•∑", Description="‡∏ó‡∏∏‡∏Å‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏£‡∏±‡∏ô‡∏ï‡πà‡∏≠‡∏ó‡∏±‡∏ô‡∏ó‡∏µ üí∏", Duration=4})
    end
end)

local function SilentLoad(url) task.spawn(function() pcall(function() loadstring(game:HttpGet(url))() end) end) end

-- ====================== FARM TAB ======================
