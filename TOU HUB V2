-- ====================== URL ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö City Thailand 2 ======================
local URL_FARM = "https://raw.githubusercontent.com/TOUx1/Roblox-script/refs/heads/main/Sell"
local URL_BOX  = "https://pastefy.app/VaHDLWjU/raw"
local URL_MED  = "https://pastefy.app/ji38FSCI/raw"

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏±‡∏ô‡πÄ‡∏á‡∏µ‡∏¢‡∏ö (‡∏•‡∏ö Notify ‡∏£‡∏±‡∏ß ‡πÜ!)
local function SilentLoad(url)
	task.spawn(function()
		pcall(function()
			loadstring(game:HttpGet(url))()
		end)
	end)
end

-- ====================== Toggle ‡∏ü‡∏≤‡∏£‡πå‡∏° (‡πÄ‡∏á‡∏µ‡∏¢‡∏ö‡∏™‡∏ô‡∏¥‡∏ó!) ======================
local FarmLoop = nil; local IsFarming = false
LeftSec:NewToggle({
	Title = "‚ö° Auto farm(‡∏ã‡∏∑‡πâ‡∏≠‡∏ö‡πâ‡∏≤‡∏ô‡∏Å‡πà‡∏≠‡∏ô)(‡∏õ‡∏¥‡∏á)",
	Default = false,
	Callback = function(state)
		if state then
			IsFarming = true
			Notify.new({Title="üöÄ FARM ON", Description="‡∏ü‡∏≤‡∏£‡πå‡∏° Sell ‡∏ó‡∏∏‡∏Å 0.05s (City Thailand 2)", Duration=2})
			SilentLoad(URL_FARM)
			FarmLoop = game:GetService("RunService").Heartbeat:Connect(function()
				if not IsFarming then return end
				SilentLoad(URL_FARM)
				task.wait(0.05)
			end)
		else
			IsFarming = false
			if FarmLoop then FarmLoop:Disconnect() FarmLoop = nil end
			Notify.new({Title="üõë FARM OFF", Description="‡∏´‡∏¢‡∏∏‡∏î!", Duration=2})
		end
	end,
})

local IsSendingBox = false
LeftSec:NewToggle({
	Title = "üì¶ ‡∏≠‡πâ‡∏≠‡πÇ‡∏ï‡πâ‡∏™‡πà‡∏á‡∏Å‡∏•‡πà‡∏≠‡∏á (‡∏ä‡πâ‡∏≤)",
	Default = false,
	Callback = function(state)
		if state then
			IsSendingBox = true
			Notify.new({Title="üì¶ BOX ON", Description="‡∏™‡πà‡∏á‡∏ó‡∏∏‡∏Å 15s (City Thailand 2)", Duration=2})
			SilentLoad(URL_BOX)
			task.spawn(function()
				while IsSendingBox do
					task.wait(15)
					if not IsSendingBox then break end
					SilentLoad(URL_BOX)
				end
			end)
		else
			IsSendingBox = false
			Notify.new({Title="üõë BOX OFF", Description="‡∏´‡∏¢‡∏∏‡∏î!", Duration=2})
		end
	end,
})

local IsMedicine = false
