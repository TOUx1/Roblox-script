local function SafeNotify(data)
	if NotifyEnabled then Notify.new(data) end
end

SafeNotify({Title="TOU HUB v34 ULTIMATE üåà", Description="‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! ‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏ô‡πÇ‡∏ö‡∏ß‡πå 2 ‡πÅ‡∏ö‡∏ö + ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß‡∏´‡∏¢‡∏∏‡∏î‡∏à‡∏£‡∏¥‡∏á + FE ‡∏î‡∏∏‡πÄ‡∏î‡∏∑‡∏≠‡∏î üí•", Duration=10})

