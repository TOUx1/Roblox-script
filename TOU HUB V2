			if not FarmStates.Sell then return end
			SilentLoad(URL_FARM)
			task.wait(0.05)
		end)
	else
		if _G.FarmLoop then _G.FarmLoop:Disconnect() _G.FarmLoop = nil end
		SafeNotify({Title="FARM OFF üò¥", Duration=2})
	end
end})

_G.BoxToggle = FarmLeft:NewToggle({Title = "‡∏≠‡πâ‡∏≠‡πÇ‡∏ï‡πâ‡∏™‡πà‡∏á‡∏Å‡∏•‡πà‡∏≠‡∏á üì¶", Default = false, Callback = function(state)
