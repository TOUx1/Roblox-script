NameSection:NewToggle({Title = "‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏£‡∏ô‡∏à‡∏±‡∏á‡∏´‡∏ß‡∏∞‡πÄ‡∏û‡∏•‡∏á üé∂", Default = false, Callback = function(state)
	IsMusicRainbow = state
	if state then
		SafeNotify({Title="‡∏ä‡∏∑‡πà‡∏≠‡∏à‡∏±‡∏á‡∏´‡∏ß‡∏∞‡πÄ‡∏û‡∏•‡∏á ON! üé∂", Description="‡∏ß‡∏¥‡πà‡∏á‡∏ï‡∏≤‡∏°‡∏ö‡∏µ‡∏ó‡∏™‡∏∏‡∏î‡πÄ‡∏ó‡πà!", Duration=4})
		MusicLoop = task.spawn(function()
			while IsMusicRainbow do
				for i=1,#musicSets do
					if not IsMusicRainbow then break end
					pcall(function() Remote:FireServer(unpack(musicSets[i])) end)
					task.wait(0.1)
				end
				for i=#musicSets,1,-1 do
					if not IsMusicRainbow then break end
					pcall(function() Remote:FireServer(unpack(musicSets[i])) end)
					task.wait(0.1)
				end
			end
		end)
	else
		if MusicLoop then task.cancel(MusicLoop) MusicLoop = nil end
		SafeNotify({Title="‡∏ä‡∏∑‡πà‡∏≠‡∏à‡∏±‡∏á‡∏´‡∏ß‡∏∞‡πÄ‡∏û‡∏•‡∏á OFF üõë", Description="‡∏´‡∏¢‡∏∏‡∏î‡∏ß‡∏¥‡πà‡∏á‡πÅ‡∏•‡πâ‡∏ß‡∏ô‡πâ‡∏≤~", Duration=2})
		pcall(function() Remote:FireServer("‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏Å‡∏ï‡∏¥", Color3.new(1,1,1), 15) end)
	end
end})

