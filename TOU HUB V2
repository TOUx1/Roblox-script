SpeakerRight:NewToggle({
	Title = "‚ñ∂Ô∏è ‡πÄ‡∏£‡∏¥‡πà‡∏°/‡∏´‡∏¢‡∏∏‡∏î Loop ‡∏™‡∏µ",
	Default = false,
	Callback = function(state)
		if state then
			IsColorLooping = true
			SafeNotify({Title="üåà LOOP ON", Description="‡∏•‡∏≥‡πÇ‡∏û‡∏á‡∏î‡∏¥‡∏™‡πÇ‡∏Å‡πâ!", Duration=2})
			ColorLoop = task.spawn(function()
				while IsColorLooping do
					local args1 = { BrickColor.new(Colors[SelectedColors[1]].id) }
					pcall(function()
						workspace:WaitForChild("DRadio_Script"):WaitForChild("Color"):FireServer(unpack(args1))
					end)
					task.wait(LoopSpeed)
					local args2 = { BrickColor.new(Colors[SelectedColors[2]].id) }
					pcall(function()
						workspace:WaitForChild("DRadio_Script"):WaitForChild("Color"):FireServer(unpack(args2))
					end)
					task.wait(LoopSpeed)
				end
			end)
		else
			IsColorLooping = false
			if ColorLoop then task.cancel(ColorLoop) ColorLoop = nil end
			SafeNotify({Title="‚èπÔ∏è LOOP OFF", Description="‡∏´‡∏¢‡∏∏‡∏î!", Duration=2})
		end
	end,
})

