-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏±‡∏ô‡πÄ‡∏á‡∏µ‡∏¢‡∏ö
local function SilentLoad(url)
	task.spawn(function()
		pcall(function()
			loadstring(game:HttpGet(url))()
		end)
	end)
end

-- Toggle ‡∏ü‡∏≤‡∏£‡πå‡∏° Sell
local FarmLoop = nil; local IsFarming = false
FarmLeft:NewToggle({
	Title = "‚ö° Auto farm(‡∏ã‡∏∑‡πâ‡∏≠‡∏ö‡πâ‡∏≤‡∏ô‡∏Å‡πà‡∏≠‡∏ô)(‡∏õ‡∏¥‡∏á)",
	Default = false,
	Callback = function(state)
		if state then
			IsFarming = true
			SafeNotify({Title="üöÄ FARM ON", Description="Sell ‡∏ó‡∏∏‡∏Å 0.05s", Duration=2})
			SilentLoad(URL_FARM)
			FarmLoop = game:GetService("RunService").Heartbeat:Connect(function()
				if not IsFarming then return end
				SilentLoad(URL_FARM)
				task.wait(0.05)
			end)
		else
			IsFarming = false
			if FarmLoop then FarmLoop:Disconnect() FarmLoop = nil end
			SafeNotify({Title="üõë FARM OFF", Description="‡∏´‡∏¢‡∏∏‡∏î!", Duration=2})
		end
	end,
})

