-- ====================== Character Safe ======================
local function GetHumanoid()
	local plr = game.Players.LocalPlayer
	local char = plr.Character or plr.CharacterAdded:Wait()
	return char:WaitForChild("Humanoid", 10)
end

local function GetRootPart()
	local plr = game.Players.LocalPlayer
	local char = plr.Character or plr.CharacterAdded:Wait()
	return char:WaitForChild("HumanoidRootPart", 10)
end

-- ====================== Auto Rejoin + Resume ======================
local TeleportService = game:GetService("TeleportService")
local PlaceId = game.PlaceId

local function RejoinAndResume()
	if not AutoRejoinEnabled then return end
	SafeNotify({Title="‡πÇ‡∏î‡∏ô Kick! üò≠", Description="‡∏Å‡∏≥‡∏•‡∏±‡∏á Rejoin + ‡∏£‡∏±‡∏ô‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡πà‡∏≠‡πÉ‡∏ô 5 ‡∏ß‡∏¥... üí®", Duration=5})
	task.wait(5)
	TeleportService:Teleport(PlaceId, game.Players.LocalPlayer)
end

game:GetService("GuiService").ErrorMessageChanged:Connect(RejoinAndResume)
TeleportService.TeleportInitFailed:Connect(RejoinAndResume)
game.Players.LocalPlayer.OnTeleport:Connect(function(state)
	if state == Enum.TeleportState.Failed then RejoinAndResume() end
end)

game.Players.LocalPlayer.CharacterAdded:Connect(function()
	if AutoResumeFarmEnabled then
		task.wait(3)
		if FarmStates.Sell and _G.SellToggle then _G.SellToggle:Callback(true) end
		if FarmStates.Box and _G.BoxToggle then _G.BoxToggle:Callback(true) end
		if FarmStates.Medicine and _G.MedToggle then _G.MedToggle:Callback(true) end
		SafeNotify({Title="‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡πÅ‡∏•‡πâ‡∏ß! ü•∞", Description="‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏£‡∏±‡∏ô‡∏ï‡πà‡∏≠‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ üí∏", Duration=4})
	end
end)

