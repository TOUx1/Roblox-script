local function TeleportTo(pos, desc)
	local root = GetRootPart()
	if root then
		local old = {}
		for _, p in pairs(root.Parent:GetDescendants()) do
			if p:IsA("BasePart") and p ~= root then
				old[p] = p.CanCollide
				p.CanCollide = false
			end
		end
		task.wait(0.1)
		root.CFrame = CFrame.new(pos)
		task.wait(0.4)
		for p, c in pairs(old) do if p and p.Parent then p.CanCollide = c end end
		SafeNotify({Title="üöÄ ‡∏ß‡∏≤‡∏õ‡πÅ‡∏•‡πâ‡∏ß!", Description=desc, Duration=1.5})
	else
		SafeNotify({Title="‚ùå Error", Description="‡∏£‡∏≠‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î!", Duration=2})
	end
end

