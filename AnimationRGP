-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°
local running = false
local remote = ReplicatedStorage:WaitForChild("BloxbizRemotes"):WaitForChild("CatalogOnApplyOutfit")

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡πà‡∏á Outfit
local function applyOutfit(args)
    remote:FireServer(unpack(args))
end

-- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏î‡∏µ‡πÄ‡∏•‡∏¢‡πå‡∏ï‡∏≤‡∏° WalkSpeed
local function getDelay()
    local ws = Character:FindFirstChild("Humanoid") and Character.Humanoid.WalkSpeed or 16
    return 16 / ws
end

-- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏´‡∏¢‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏ô
local function isStopped()
    local hum = Character:FindFirstChild("HumanoidRootPart")
    if hum then
        return hum.Velocity.Magnitude < 1
    end
    return false
end

