--// ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏±‡∏ô outfit ‡∏ï‡∏≤‡∏°‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local function getDelay()
    local humanoid = Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        local speed = humanoid.WalkSpeed
        return 16 / speed -- ‡∏ñ‡πâ‡∏≤ WalkSpeed =16 ‡∏à‡∏∞ delay=1s
    else
        return 1
    end
end

local runningLoop = false

startButton.MouseButton1Click:Connect(function()
    runningLoop = true
    spawn(function()
        while runningLoop do
            for _, outfit in ipairs(outfits) do
                if not runningLoop then break end
                applyOutfit(outfit)
                wait(getDelay())
            end
        end
    end)
end)

stopButton.MouseButton1Click:Connect(function()
    runningLoop = false
end)

--// ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ö‡∏≠‡∏Å‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, 0, 0, 20)
statusLabel.Position = UDim2.new(0, 0, 0.05, 0)
statusLabel.BackgroundTransparency = 1
statusLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
statusLabel.Font = Enum.Font.SourceSansBold
statusLabel.TextSize = 16
statusLabel.Text = "Status: Stopped"
statusLabel.Parent = mainFrame

spawn(function()
    while true do
        if runningLoop then
            statusLabel.Text = "Status: Running"
        else
            statusLabel.Text = "Status: Stopped"
        end
        wait(0.2)
    end
end)
