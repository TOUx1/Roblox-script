local function runStoppedOutfit()
    local stoppedOutfit = {
        {
            SwimAnimation = 0,
            DepthScale = 1,
            RightLegColor = Color3.new(0.7411765, 0.7372549, 0.7411765),
            MoodAnimation = 0,
            Face = 51241171,
            JumpAnimation = 0,
            HeadColor = Color3.new(0.7411765, 0.7372549, 0.7411765),
            BodyTypeScale = 0.3,
            ClimbAnimation = 0,
            LeftArmColor = Color3.new(0.7411765, 0.7372549, 0.7411765),
            LeftLegColor = Color3.new(0.7411765, 0.7372549, 0.7411765),
            Pants = 17761081613,
            RightArmColor = Color3.new(0.7411765, 0.7372549, 0.7411765),
            WidthScale = 1,
            LeftArm = 27112052,
            IdleAnimation = 0,
            RightArm = 27112039,
            GraphicTShirt = 0,
            Head = 0,
            Shirt = 71072007171100,
            Torso = 27112025,
            RunAnimation = 0,
            WalkAnimation = 0,
            FallAnimation = 0,
            TorsoColor = Color3.new(0.7411765, 0.7372549, 0.7411765),
            Accessories = {
                {Rotation=Vector3.new(), AssetId=10928435480, Position=Vector3.new(), Scale=Vector3.new(1,1,1), IsLayered=false, AccessoryType=Enum.AccessoryType.Hair},
                {Rotation=Vector3.new(), AssetId=13641422551, Position=Vector3.new(), Scale=Vector3.new(1,1,1), IsLayered=false, AccessoryType=Enum.AccessoryType.Face}
            },
            RightLeg = 27112068,
            HeadScale = 1,
            HeightScale = 1,
            ProportionScale = 1,
            LeftLeg = 27112056
        }
    }
    applyOutfit(stoppedOutfit)
end

-- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ó‡∏∏‡∏Å frame ‡∏ñ‡πâ‡∏≤‡∏´‡∏¢‡∏∏‡∏î‡πÄ‡∏î‡∏¥‡∏ô‡∏à‡∏∞‡∏£‡∏±‡∏ô outfit ‡∏™‡∏∏‡∏î‡∏ó‡πâ‡∏≤‡∏¢
RunService.RenderStepped:Connect(function()
    if Character:FindFirstChild("Humanoid") and Character.Humanoid.MoveDirection.Magnitude == 0 and not running then
        runStoppedOutfit()
    end
end)
