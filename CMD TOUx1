local function updateDrag(input)
    antiDebug()
    local delta = input.Position - q4f6t2m1
    b4z7q0n1.Position = UDim2.new(
        h8r4v9x3.X.Scale, h8r4v9x3.X.Offset + delta.X,
        h8r4v9x3.Y.Scale, h8r4v9x3.Y.Offset + delta.Y
    )
end

b4z7q0n1.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 
    or input.UserInputType == Enum.UserInputType.Touch then
        x5k2p8j4 = true
        q4f6t2m1 = input.Position
        h8r4v9x3 = b4z7q0n1.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                x5k2p8j4 = false
            end
        end)
    end
end)

b4z7q0n1.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement 
    or input.UserInputType == Enum.UserInputType.Touch then
        y7n0w3z9 = input
    end
end)

