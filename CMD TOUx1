-- XOR Encryption
local function xor(str, key)
    local result = ""
    for i = 1, #str do
        result = result .. string.char(bit32.bxor(string.byte(str, i), key))
    end
    return result
end

-- Random key per run
math.randomseed(tick())
local xorkey = math.random(1, 255)
local enc = function(s) return xor(s, xorkey) end
local dec = function(s) return xor(s, xorkey) end

-- Anti-debug
local function antiDebug()
    if debug.getinfo or getfenv or _G.TamperCheck then
        error(dec(enc("Unauthorized access detected!")))
    end
end

-- Junk table
local junk = {
    _0x7a2b3c = function() return math.random(1000, 9999) end,
    _0x9d5e6f = {data = "useless", value = math.random()},
    _0x4g8h9i = function() for i=1,50 do math.random() end end
}

