-- Player page
local function showPlayer()
    _G.PageFunctions.clearContent()
    local speedLabel = Instance.new(dec(enc("TextLabel")))
    speedLabel.Size = UDim2.new(0,150,0,30)
    speedLabel.Position = UDim2.new(0,10,0,20)
    speedLabel.BackgroundTransparency = 1
    speedLabel.Text = dec(enc("ความเร็ว: "))..game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).WalkSpeed
    speedLabel.TextColor3 = Color3.fromRGB(255,255,255)
    speedLabel.TextSize = 18
    speedLabel.Font = Enum.Font.Gotham
    speedLabel.TextXAlignment = Enum.TextXAlignment.Left
    speedLabel.Parent = _G.GUIComponents.contentFrame

    local speedBox = Instance.new(dec(enc("TextBox")))
    speedBox.Size = UDim2.new(0,80,0,30)
    speedBox.Position = UDim2.new(0,170,0,20)
    speedBox.BackgroundColor3 = Color3.fromRGB(128,128,128)
    speedBox.Text = tostring(game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).WalkSpeed)
    speedBox.TextColor3 = Color3.fromRGB(255,255,255)
    speedBox.TextSize = 18
    speedBox.Parent = _G.GUIComponents.contentFrame

    speedBox.FocusLost:Connect(function(enter)
        if enter then
            local newSpeed = tonumber(speedBox.Text)
            if newSpeed and newSpeed > 0 and newSpeed <= 500 then
                game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).WalkSpeed = newSpeed
                speedLabel.Text = dec(enc("ความเร็ว: "))..newSpeed
            else
                speedBox.Text = tostring(game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).WalkSpeed)
            end
        end
    end)

    local jumpLabel = Instance.new(dec(enc("TextLabel")))
    jumpLabel.Size = UDim2.new(0,150,0,30)
    jumpLabel.Position = UDim2.new(0,10,0,60)
    jumpLabel.BackgroundTransparency = 1
    jumpLabel.Text = dec(enc("กระโดด: "))..game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).JumpPower
    jumpLabel.TextColor3 = Color3.fromRGB(255,255,255)
    jumpLabel.TextSize = 18
    jumpLabel.Font = Enum.Font.Gotham
    jumpLabel.TextXAlignment = Enum.TextXAlignment.Left
    jumpLabel.Parent = _G.GUIComponents.contentFrame

    local jumpBox = Instance.new(dec(enc("TextBox")))
    jumpBox.Size = UDim2.new(0,80,0,30)
    jumpBox.Position = UDim2.new(0,170,0,60)
    jumpBox.BackgroundColor3 = Color3.fromRGB(128,128,128)
    jumpBox.Text = tostring(game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).JumpPower)
    jumpBox.TextColor3 = Color3.fromRGB(255,255,255)
    jumpBox.TextSize = 18
    jumpBox.Parent = _G.GUIComponents.contentFrame

    jumpBox.FocusLost:Connect(function(enter)
        if enter then
            local newJump = tonumber(jumpBox.Text)
            if newJump and newJump > 0 and newJump <= 500 then
                game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).JumpPower = newJump
                jumpLabel.Text = dec(enc("กระโดด: "))..newJump
            else
                jumpBox.Text = tostring(game.Players.LocalPlayer.Character:WaitForChild(dec(enc("Humanoid"))).JumpPower)
            end
        end
    end)
end

-- Script page
local function showScript()
    _G.PageFunctions.clearContent()
    local pumpLabel = Instance.new(dec(enc("TextLabel")))
    pumpLabel.Size = UDim2.new(0,100,0,30)
    pumpLabel.Position = UDim2.new(0,10,0,20)
    pumpLabel.BackgroundTransparency = 1
    pumpLabel.Text = dec(enc("ปั้มเงิน"))
    pumpLabel.TextColor3 = Color3.fromRGB(255,255,255)
    pumpLabel.TextSize = 18
    pumpLabel.Font = Enum.Font.Gotham
    pumpLabel.TextXAlignment = Enum.TextXAlignment.Left
    pumpLabel.Parent = _G.GUIComponents.contentFrame

    local toggleButton = Instance.new(dec(enc("TextButton")))
    toggleButton.Size = UDim2.new(0,80,0,30)
    toggleButton.Position = UDim2.new(0,120,0,20)
    toggleButton.BackgroundColor3 = Color3.fromRGB(60,60,60)
    toggleButton.Text = dec(enc("OFF"))
    toggleButton.TextColor3 = Color3.fromRGB(255,255,255)
    toggleButton.TextSize = 18
    toggleButton.Parent = _G.GUIComponents.contentFrame

    toggleButton.MouseButton1Click:Connect(function()
        _G.AutoFarm = not _G.AutoFarm
        toggleButton.Text = _G.AutoFarm and dec(enc("ON")) or dec(enc("OFF"))
        spawn(function()
            while _G.AutoFarm do
                local args = {dec(enc("Sell"))}
                game:GetService(dec(enc("ReplicatedStorage"))):WaitForChild(dec(enc("House"))):FireServer(unpack(args))
                wait(0.1)
            end
        end)
    end)
end

