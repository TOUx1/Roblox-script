local textBox = Instance.new("TextBox", frameNormal)
textBox.PlaceholderText = "‡πÉ‡∏™‡πà‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á"
textBox.Size = UDim2.new(0.8, 0, 0, 40)
