-- Chat commands
local tqrf5jsu = game:GetService(dec(enc("TextChatService")))
local function sendMessage(msg)
    local channel = tqrf5jsu:FindFirstChild(dec(enc("TextChannels"))) and tqrf5jsu.TextChannels:FindFirstChild(dec(enc("RBXGeneral")))
    if channel then
        channel:SendAsync(msg)
    else
        warn(dec(enc("❌ ไม่พบแชนเนล RBXGeneral")))
    end
end
sendMessage(dec(enc("SCRIPT CITY THAILAND 2 BY:TOUx1")))

local q9x7z2k1 = game:GetService(dec(enc("Players")))
local sqocc0br = game:GetService(dec(enc("RunService")))
local csvpzk8q = q9x7z2k1[dec(enc("LocalPlayer"))]

local gr5v8den = dec(enc("birdV2_123"))
local eqjk8hvr = false
local 7ifbwzf9 = 0
local tmlocih0 = 10
local yifponzw = 3

local function vugrtgao()
    local args = {dec(enc("134647289919930"))}
    local radio = workspace:FindFirstChild(dec(enc("DRadio_Script")))
    if radio and radio:FindFirstChild(dec(enc("Event"))) then
        radio.Event:FireServer(unpack(args))
    end
end

local function gi6b3zab(targetChar)
    if eqjk8hvr then return end
    eqjk8hvr = true
    vugrtgao()
    sqocc0br.Heartbeat:Connect(function(dt)
        if eqjk8hvr and targetChar and targetChar:FindFirstChild(dec(enc("HumanoidRootPart"))) 
        and csvpzk8q.Character and csvpzk8q.Character:FindFirstChild(dec(enc("HumanoidRootPart"))) then
            7ifbwzf9 = 7ifbwzf9 + yifponzw * dt
            local x = math.cos(7ifbwzf9) * tmlocih0
            local z = math.sin(7ifbwzf9) * tmlocih0
