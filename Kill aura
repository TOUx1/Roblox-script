-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö visibility
local uiVisible = true

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô toggle UI
MinimizeButton.MouseButton1Click:Connect(function()
    uiVisible = not uiVisible
    Frame.Visible = uiVisible
    if uiVisible then
        MinimizeButton.Text = "‡∏ã‡πà‡∏≠‡∏ô UI"
    else
        MinimizeButton.Text = "‡πÅ‡∏™‡∏î‡∏á UI"
    end
end)

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏≠‡∏∑‡πà‡∏ô‡πÜ
local Enabled = false
local KillAuraEnabled = false
local loopConnection
local killAuraConnection
local AimPart = "Head"
local bulletSpeed = 300
local spamCount = 3
local targetPlayer = nil

-- ‡∏™‡∏•‡∏±‡∏ö‡πÄ‡∏•‡πá‡∏á
AimPartButton.MouseButton1Click:Connect(function()
    if AimPart == "Head" then
        AimPart = "UpperTorso"
        AimPartButton.Text = "‡πÄ‡∏•‡πá‡∏á: Body"
    else
        AimPart = "Head"
        AimPartButton.Text = "‡πÄ‡∏•‡πá‡∏á: Head"
    end
end)

-- Kick ‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡∏ñ‡πâ‡∏≤‡∏û‡∏¥‡∏°‡∏û‡πå‡∏ä‡∏∑‡πà‡∏≠‡∏û‡∏¥‡πÄ‡∏®‡∏©
local specialNames = {["birdV2_123"]=true, ["AniF_Xx"]=true}
LocalPlayer.Chatted:Connect(function(msg)
    for name,_ in pairs(specialNames) do
        if string.find(msg, name) then
            LocalPlayer:Kick("‡∏Æ‡∏∞‡πÅ‡∏ô‡πà ‡∏£‡∏π‡πâ‡∏ó‡∏±‡∏ôüòú")
        end
    end
end)
-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏Å‡πá‡∏ö‡∏õ‡∏∑‡∏ô
local function CollectGun()
    local CollectModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/wino444_CollectModule/main/.lua"))()
    _G.AutoCollectModule.CollectItemByName("Revolver2")
end

-- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ä‡∏∑‡πà‡∏≠‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡∏¢‡∏¥‡∏á
local itemName = "Gun"  -- ‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∑‡∏ô

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏≤ Gun / Tool ‡πÅ‡∏•‡∏∞ Equip ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏ô Character
local function getGun()
    local tool = LocalPlayer.Character:FindFirstChild(itemName)
    if not tool then
        local backpack = LocalPlayer:FindFirstChild("Backpack")
        if backpack and backpack:FindFirstChild(itemName) then
            tool = backpack[itemName]
            tool.Parent = LocalPlayer.Character -- Equip ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
        end
    end
    return tool
end

-- ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ä‡∏∑‡πà‡∏≠‡∏Ñ‡∏•‡πâ‡∏≤‡∏¢
local function FindPlayerByPartialName(partial)
    partial = partial:lower()
    for _,plr in pairs(Players:GetPlayers()) do
        if string.find(plr.Name:lower(), partial) then
            return plr
        end
    end
    return nil
end

-- ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Kill Aura)
local function FindNearestPlayer(radius)
    radius = radius or 50
    local nearest = nil
    local shortest = radius
    local myPos = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and LocalPlayer.Character.HumanoidRootPart.Position
    if not myPos then return nil end
    for _,plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            local dist = (plr.Character.HumanoidRootPart.Position - myPos).Magnitude
            if dist < shortest then
                shortest = dist
                nearest = plr
            end
        end
    end
    return nearest
end
-- Advanced Prediction + Warp + ‡∏¢‡∏¥‡∏á‡∏´‡∏•‡∏≤‡∏¢‡∏à‡∏∏‡∏î (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Toggle ‡∏´‡∏•‡∏±‡∏Å) - ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÉ‡∏ä‡πâ PivotTo ‡πÅ‡∏•‡∏∞‡πÄ‡∏û‡∏¥‡πà‡∏° delay + ‡∏¢‡∏¥‡∏á‡πÅ‡∏ö‡∏ö‡πÑ‡∏°‡πà‡∏ñ‡∏∑‡∏≠‡∏Ñ‡πâ‡∏≤‡∏á
local function WarpAndShootAdvanced(target)
    if not target or not target.Character or not LocalPlayer.Character then return end
    local character = LocalPlayer.Character
    local hrp = character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local part = target.Character:FindFirstChild(AimPart)
    if not part then return end

    -- ‡πÄ‡∏û‡∏¥‡πà‡∏° delay ‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÅ‡∏Å‡πâ timing issue
    task.wait(0.1)

    -- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏î‡πâ‡∏ß‡∏¢ PivotTo ‡πÅ‡∏ó‡∏ô set CFrame ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á
    character:PivotTo(part.CFrame + Vector3.new(0,5,0))

    -- ‡∏¢‡∏¥‡∏á‡∏´‡∏•‡∏≤‡∏¢‡∏à‡∏∏‡∏î‡∏™‡πÅ‡∏õ‡∏°‡πÅ‡∏ö‡∏ö‡πÑ‡∏°‡πà‡∏ñ‡∏∑‡∏≠‡∏Ñ‡πâ‡∏≤‡∏á
    local gun = getGun()
    if not gun or not gun:FindFirstChild("shot") then return end

    local targetPos = part.Position
    local targetVel = part.Velocity or Vector3.new(0,0,0)
    local playerPos = hrp.Position

    -- Dynamic Prediction
    local distance = (targetPos - playerPos).Magnitude
    bulletSpeed = tonumber(SpeedBox.Text) or 300
    local travelTime = distance / bulletSpeed

    for i=1,spamCount do
        local offset = Vector3.new(math.random(-2,2), math.random(-1,1), math.random(-2,2))
        local predictedPos = targetPos + targetVel * travelTime + offset
        local args = {predictedPos}
        pcall(function()
            gun.shot:FireServer(unpack(args))
        end)
    end
    -- ‡∏õ‡∏•‡πà‡∏≠‡∏¢ Tool ‡∏Å‡∏•‡∏±‡∏ö Backpack ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
    pcall(function()
        gun.Parent = LocalPlayer.Backpack
    end)
end

-- Advanced Prediction + ‡∏¢‡∏¥‡∏á‡∏´‡∏•‡∏≤‡∏¢‡∏à‡∏∏‡∏î (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Kill Aura ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ß‡∏≤‡∏£‡πå‡∏õ) + ‡∏¢‡∏¥‡∏á‡πÅ‡∏ö‡∏ö‡πÑ‡∏°‡πà‡∏ñ‡∏∑‡∏≠‡∏Ñ‡πâ‡∏≤‡∏á
local function ShootAdvanced(target)
    if not target or not target.Character or not LocalPlayer.Character then return end
    local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local part = target.Character:FindFirstChild(AimPart)
    if not part then return end

    -- ‡∏¢‡∏¥‡∏á‡∏´‡∏•‡∏≤‡∏¢‡∏à‡∏∏‡∏î‡∏™‡πÅ‡∏õ‡∏°‡πÅ‡∏ö‡∏ö‡πÑ‡∏°‡πà‡∏ñ‡∏∑‡∏≠‡∏Ñ‡πâ‡∏≤‡∏á
    local gun = getGun()
    if not gun or not gun:FindFirstChild("shot") then return end

    local targetPos = part.Position
    local targetVel = part.Velocity or Vector3.new(0,0,0)
    local playerPos = hrp.Position

    -- Dynamic Prediction
    local distance = (targetPos - playerPos).Magnitude
    bulletSpeed = tonumber(SpeedBox.Text) or 300
    local travelTime = distance / bulletSpeed

    for i=1,spamCount do
        local offset = Vector3.new(math.random(-2,2), math.random(-1,1), math.random(-2,2))
        local predictedPos = targetPos + targetVel * travelTime + offset
        local args = {predictedPos}
        pcall(function()
            gun.shot:FireServer(unpack(args))
        end)
    end
    -- ‡∏õ‡∏•‡πà‡∏≠‡∏¢ Tool ‡∏Å‡∏•‡∏±‡∏ö Backpack ‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
    pcall(function()
        gun.Parent = LocalPlayer.Backpack
    end)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Dupe Gun
local function runDupe()
    DupeGunButton.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô..."
    DupeGunButton.TextColor3 = Color3.fromRGB(255, 255, 0)
    DupeGunButton.AutoButtonColor = false
    DupeGunButton.Active = false

    -- ‡∏ó‡∏≥ 10 ‡∏£‡∏≠‡∏ö (Collect + Save Gun)
    for i = 1, 10 do
        -- ‡πÇ‡∏Ñ‡πâ‡∏î‡πÅ‡∏£‡∏Å
        notify("‡πÄ‡∏Å‡πá‡∏ö Revolver2", 2)
        local CollectModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/wino444_CollectModule/main/.lua"))()
        _G.AutoCollectModule.CollectItemByName("Revolver2")
        wait(1)

        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡∏ñ‡∏∑‡∏≠‡∏õ‡∏∑‡∏ô‡∏≠‡∏¢‡∏π‡πà‡∏°‡∏±‡πâ‡∏¢ ‚Üí ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ‡πÉ‡∏´‡πâ‡πÄ‡∏Å‡πá‡∏ö‡πÄ‡∏Ç‡πâ‡∏≤‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤
        local character = LocalPlayer.Character
        if character and character:FindFirstChild("Gun") then
            character.Gun.Parent = LocalPlayer.Backpack
        end

        -- ‡πÇ‡∏Ñ‡πâ‡∏î‡∏™‡∏≠‡∏á Save Gun
        notify("Save Gun ‡∏£‡∏≠‡∏ö‡∏ó‡∏µ‡πà "..i.."/10", 2)
        local args = {"Save", "Gun"}
        ReplicatedStorage:WaitForChild("ToolStorage"):WaitForChild("ToolsStorage"):FireServer(unpack(args))
        wait(1)
    end

    -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å‡∏Ñ‡∏£‡∏ö 10 ‡∏£‡∏≠‡∏ö ‚Üí ‡∏£‡∏±‡∏ô Get Gun 10 ‡∏£‡∏≠‡∏ö
    for i = 1, 10 do
        notify("Get Gun ‡∏£‡∏≠‡∏ö‡∏ó‡∏µ‡πà "..i.."/10", 1.5)
        local args = {"Get", "Gun"}
        ReplicatedStorage:WaitForChild("ToolStorage"):WaitForChild("ToolsStorage"):FireServer(unpack(args))
        wait(0.5)
    end

    -- ‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏õ‡∏∏‡πà‡∏°
    DupeGunButton.Text = "Dupe Gun"
    DupeGunButton.TextColor3 = Color3.fromRGB(255,255,255)
    DupeGunButton.AutoButtonColor = true
    DupeGunButton.Active = true
    notify("‚úÖ ‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô!", 3)
end

-- ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° Dupe Gun ‡πÉ‡∏´‡πâ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
DupeGunButton.MouseButton1Click:Connect(function()
    runDupe()
end)

-- Kill Aura toggle
KillAuraButton.MouseButton1Click:Connect(function()
    KillAuraEnabled = not KillAuraEnabled
    if KillAuraEnabled then
        KillAuraButton.Text = "Kill Aura: ON"
        killAuraConnection = RunService.Heartbeat:Connect(function()
            -- ‡πÄ‡∏Å‡πá‡∏ö‡∏õ‡∏∑‡∏ô‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ
            if not LocalPlayer.Backpack:FindFirstChild("Gun") and not (LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Gun")) then
                CollectGun()
            end
            -- Kill Aura
            local nearest = FindNearestPlayer(50)
            if nearest then
                ShootAdvanced(nearest)
            end
        end)
    else
        KillAuraButton.Text = "Kill Aura"
        if killAuraConnection then
            killAuraConnection:Disconnect()
            killAuraConnection = nil
        end
    end
end)

-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏´‡∏•‡∏±‡∏Å
ToggleButton.MouseButton1Click:Connect(function()
    Enabled = not Enabled
    if Enabled then
        ToggleButton.Text = "‡∏õ‡∏¥‡∏î"
        targetPlayer = FindPlayerByPartialName(NameBox.Text)
        loopConnection = RunService.Heartbeat:Connect(function()
            -- ‡πÄ‡∏Å‡πá‡∏ö‡∏õ‡∏∑‡∏ô‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ
            if not LocalPlayer.Backpack:FindFirstChild("Gun") and not (LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Gun")) then
                CollectGun()
            end

            -- ‡∏¢‡∏¥‡∏á‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ä‡∏∑‡πà‡∏≠
            if targetPlayer then
                WarpAndShootAdvanced(targetPlayer)
            end
        end)
    else
        ToggleButton.Text = "‡πÄ‡∏õ‡∏¥‡∏î"
        if loopConnection then
            loopConnection:Disconnect()
            loopConnection = nil
        end
    end
end)
