UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0, 5)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
local function notify(msg, duration)
    local note = Instance.new("TextLabel")
    note.Size = UDim2.new(1, 0, 0, 30)
    note.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    note.Text = msg
    note.TextColor3 = Color3.fromRGB(255, 255, 255)
    note.Font = Enum.Font.SourceSansBold
    note.TextSize = 20
    note.Parent = notifyFrame

    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 8)
    UICorner.Parent = note

    task.spawn(function()
        wait(duration or 2)
        note:Destroy()
    end)
end

-- ‡∏õ‡∏∏‡πà‡∏°‡∏ã‡πà‡∏≠‡∏ô/‡πÅ‡∏™‡∏î‡∏á UI (‡πÅ‡∏¢‡∏Å‡∏ô‡∏≠‡∏Å Frame ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏¢‡∏±‡∏á‡∏Ñ‡∏•‡∏¥‡∏Å‡πÑ‡∏î‡πâ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ã‡πà‡∏≠‡∏ô)
